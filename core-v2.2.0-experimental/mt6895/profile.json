{
  "platform": "mt6895",
  "platform_name": "D8100",
  "framework": 220,
  "alias": {
    "boost_affinity": "/sys/module/mtk_fpsgo/parameters/boost_affinity",
    "uclamp_heavy": "/dev/cpuctl/heavy/cpu.uclamp.min",
    "uclamp_heavy_max": "/dev/cpuctl/heavy/cpu.uclamp.max",
    "ddr_freq_min": "/sys/devices/platform/soc/1c00f000.dvfsrc/mtk-dvfsrc-devfreq/devfreq/mtk-dvfsrc-devfreq/min_freq",
    "ddr_freq_max": "/sys/devices/platform/soc/1c00f000.dvfsrc/mtk-dvfsrc-devfreq/devfreq/mtk-dvfsrc-devfreq/max_freq",
    "ddr_freq_cust": "/sys/class/devfreq/mtk-dvfsrc-devfreq/userspace/set_freq",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig",
    "ged_kpi": "/sys/module/sspm_v3/holders/ged/parameters/is_GED_KPI_enabled",
    "fpsgo": "/sys/kernel/fpsgo/common/fpsgo_enable",
    "charge_limit": "/sys/class/power_supply/battery/charge_control_limit",
    "night_charging": "/sys/class/power_supply/battery/night_charging",
    "cpufreq": "/proc/cpudvfs/cpufreq_debug",
    "perfmgr_enable": "/sys/module/mtk_fpsgo/parameters/perfmgr_enable",
    "ddr_max": "/sys/class/devfreq/mtk-dvfsrc-devfreq/max_freq"
  },
  "features": {
    "high_rate": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_refresh_rate",
      "device_policy": [{
        "device": "rubens", "name": "K50", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0", "mode_standby": "1",
        "1440P60Hz": "0", "1440P120Hz": "1", "1080P120Hz": "2", "1080P60Hz": "3"
      }, {
        "device": "OP5565", "name": "OnePlus ACE", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0"
      }]
    },
    "cpus_control": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_cpus_control"
    },
    "charge_control": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_charge_control",
      "suspend": [
        ["$charge_limit", "15"],
        ["$night_charging", "1"]
      ],
      "normal": [
        ["$charge_limit", "0"],
        ["$night_charging", "0"]
      ]
    },
    "limiter": {
      "active_interval": 20,
      "inactive_interval": 150,
      "logger": false,
      "params": [
        { "id": "p1:cpu4", "max_freq": 2100000, "min_freq": 1800000, "margin_mhz": 350 },
        { "id": "p1:cpu7", "max_freq": 2400000, "min_freq": 2100000, "margin_mhz": 300 },

        { "id": "p2:cpu4", "max_freq": 2450000, "min_freq": 2200000, "margin_mhz": 650 },
        { "id": "p2:cpu7", "max_freq": 2650000, "min_freq": 2450000, "margin_mhz": 650 },

        { "id": "p3:cpu4", "max_freq": 2650000, "min_freq": 2450000, "margin_mhz": 650 },
        { "id": "p3:cpu7", "max_freq": 2850000, "min_freq": 2650000, "margin_mhz": 650 },


        { "id": "g1:cpu4", "max_freq": 2450000, "min_freq": 2000000, "margin_mhz": 300 },
        { "id": "g1:cpu7", "max_freq": 2450000, "min_freq": 2000000, "margin_mhz": 400 },

        { "id": "g2:cpu4", "max_freq": 2650000, "min_freq": 2450000, "margin_mhz": 300 },
        { "id": "g2:cpu7", "max_freq": 2650000, "min_freq": 2450000, "margin_mhz": 400 },

        { "id": "g3:cpu4", "max_freq": 2850000, "min_freq": 2650000, "margin_mhz": 300 },
        { "id": "g3:cpu7", "max_freq": 2850000, "min_freq": 2650000, "margin_mhz": 400 },

        { "id": "g4:cpu4", "max_freq": 3000000, "min_freq": 2650000, "margin_mhz": 400 },
        { "id": "g4:cpu7", "max_freq": 3100000, "min_freq": 2650000, "margin_mhz": 400 },


        { "id": "inactive:cpu0", "max_freq": 1800000, "min_freq": 1000000, "margin_mhz": 200 },
        { "id": "inactive:cpu4", "max_freq": 1900000, "min_freq": 400000, "margin_mhz": 180 },
        { "id": "inactive:cpu7", "max_freq": 2300000, "min_freq": 500000, "margin_mhz": 220 },

        { "id": "idle:cpu0", "max_freq": 1600000, "min_freq": 600000, "margin_mhz": 120 },
        { "id": "idle:cpu4", "max_freq": 1800000, "min_freq": 400000, "margin_mhz": 100 },
        { "id": "idle:cpu7", "max_freq": 2000000, "min_freq": 500000, "margin_mhz": 120 }
      ]
    },
    "gesture_boost": {
      "enable": true,
      "enter": [ ["$ddr_freq_cust", "5500000000"] ],
      "exit": [["$ddr_freq_cust", "#0"] ]
    }
  },
  "presets": {
    "reset": [
      ["@mtk_reset"],
      ["@governor", "schedutil"],
      ["$uclamp_heavy_max", "max"],
      ["$perfmgr_enable", "#0"],
      ["$boost_affinity", "#0"]
    ],
    "min_freq": [
      ["$cpu_min_0"], ["$cpu_min_4"], ["$cpu_min_7"]
    ],
    "max_freq": [
      ["$cpu_max_0"], ["$cpu_max_4"], ["$cpu_max_7"]
    ],
    "kpi_on": [
      ["$ged_kpi", "#1"],
      ["$fpsgo", "#1"]
    ],
    "kpi_off": [
      ["$fpsgo", "#0"],
      ["$ged_kpi", "#0"]
    ],
    "common_actvie": [
      ["$ddr_max", "#5500000000"],
      ["$cpu_max_0", "2000000"],
      ["@high_rate", "on"]
    ],
    "common_inactvie": [
      ["$ddr_freq_min", "#800000000"],
      ["/sys/kernel/ged/hal/custom_boost_gpu_freq", "99"]
    ],
    "common_app": [
      ["/sys/kernel/cm_mgr/dbg_cm_mgr", "cm_mgr_enable 0|cm_mgr_perf_enable 0"],
      ["@cpuset", "0-3", "0-7", "0-7", "0-7"],
      ["/sys/kernel/ged/hal/timer_base_dvfs_margin", "#30"],
      ["/sys/kernel/ged/hal/loading_base_dvfs_step", "#6"],
      ["$ddr_freq_cust", "0"]
    ],
    "common_gaming": [
      ["$uclamp_heavy", "0"],
      ["@preset", "kpi_off"],
      ["/sys/kernel/cm_mgr/dbg_cm_mgr", "cm_mgr_enable 1|cm_mgr_perf_enable 1"],
      ["$ddr_max", "#5500000000"],
      ["/sys/kernel/ged/hal/custom_upbound_gpu_freq", "#0"],
      ["/sys/kernel/ged/hal/custom_boost_gpu_freq", "99"],
      ["/sys/module/ged/parameters/gpu_cust_boost_freq",  "0"],
      ["/sys/kernel/ged/hal/loading_base_dvfs_step", "#4"],
      ["/sys/kernel/ged/hal/timer_base_dvfs_margin", "#15"],
      ["@cpuset", "0-2", "1-3", "0-5", "0-7"],
      ["$ddr_freq_cust", "0"]
    ],

    "powersave_active": [
      ["@limiters", "p1:cpu4", "p1:cpu7"],
      ["@values", "min_freq", "800000", "500000", "600000"],
      ["$ddr_freq_min", "#2133000000"],
      ["@preset", "common_actvie"]
    ],
    "powersave_inactive": [
      ["@limiters", "idle:cpu0", "idle:cpu4", "idle:cpu7"],
      ["@values", "min_freq", "450000", "200000", "500000"],
      ["@high_rate", "auto"],
      ["@preset", "common_inactvie"],
      ["$ddr_max", "800000000"]
    ],
    "balance_active": [
      ["@limiters", "p2:cpu4", "p2:cpu7"],
      ["@values", "min_freq", "1000000", "900000", "800000"],
      ["$ddr_freq_min", "#2133000000"],
      ["@preset", "common_actvie"]
    ],
    "balance_inactive": [
      ["@limiters", "inactive:cpu0", "inactive:cpu4", "inactive:cpu7"],
      ["@values", "min_freq", "450000", "200000", "500000"],
      ["@preset", "common_inactvie"],
      ["@high_rate", "auto"]
    ],
    "performance_active": [
      ["@limiters", "p3:cpu4", "p3:cpu7"],
      ["@values", "min_freq", "1200000", "1600000", "1600000"],
      ["$ddr_freq_min", "3200000000"],
      ["@preset", "common_actvie"]
    ],
    "performance_inactive": [
      ["@limiters", "p2:cpu4", "p2:cpu7"],
      ["@values", "min_freq", "450000", "400000", "500000"],
      ["@preset", "common_inactvie"],
      ["@high_rate", "auto"]
    ],
    "fast_active": [
      ["@cpu_freq", "policy0", "1200000", "2000000"],
      ["@cpu_freq", "policy4", "1600000", "2850000"],
      ["@cpu_freq", "policy7", "1600000", "2850000"],
      ["$ddr_freq_min", "3200000000"],
      ["@preset", "common_actvie"]
    ],
    "fast_inactive": [
      ["@limiters", "p3:cpu4", "p3:cpu7"],
      ["@values", "min_freq", "450000", "400000", "500000"],
      ["@preset", "common_inactvie"],
      ["@high_rate", "auto"]
    ],
    "pedestal_active": [
      ["@limiters", "NONE"],
      ["@cpu_freq", "policy0", "1800000", "2000000"],
      ["@cpu_freq", "policy4", "1800000", "2850000"],
      ["@cpu_freq", "policy7", "2100000", "2850000"],
      ["$ddr_freq_min", "5500000000"],
      ["@preset", "common_actvie"]
    ],
    "pedestal_inactive": [
      ["@cpu_freq", "policy0", "1600000", "2000000"],
      ["@cpu_freq", "policy4", "1600000", "2850000"],
      ["@cpu_freq", "policy7", "1600000", "2850000"],
      ["@preset", "common_inactvie"]
    ]
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "powersave_active"],
        ["$uclamp_heavy", "0"],
        ["@charge", "normal"]
      ],
      "app": [
        ["@preset", "comm_app"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["@limiters", "NONE"],
        ["@values", "min_freq", "450000", "400000", "500000"],
        ["@values", "max_freq", "#2000000", "#1900000", "#2350000"],
        ["@values", "min_freq", "#450000", "#400000", "#500000"]
      ]
    },
    "balance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "balance_active"],
        ["@charge", "normal"]
      ],
      "app": [
        ["@preset", "comm_app"],
        ["$uclamp_heavy", "1.00"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["@limiters", "NONE"],
        ["@values", "min_freq", "450000", "400000", "500000"],
        ["@values", "max_freq", "#2000000", "#2250000", "#2650000"],
        ["@values", "min_freq", "#450000", "#400000", "#500000"]
      ]
    },
    "performance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "performance_active"],
        ["$uclamp_heavy", "1.00"]
      ],
      "app": [
        ["@preset", "comm_app"],
        ["@charge", "normal"],
        ["$uclamp_heavy", "1.00"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["@charge", "suspend"],
        ["@limiters", "NONE"],
        ["@values", "min_freq", "800000", "700000", "900000"],
        ["@values", "max_freq", "#2000000", "#2650000", "#2850000"],
        ["@values", "min_freq", "#800000", "#700000", "#900000"]
      ]
    },
    "fast": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "fast_active"]
      ],
      "app": [
        ["@preset", "comm_app"],
        ["@charge", "normal"],
        ["$uclamp_heavy", "1.00"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["@charge", "suspend"],
        ["$mi_thermal", "10"],
        ["@limiters", "NONE"],
        ["@values", "min_freq", "1600000", "1800000", "1800000"],
        ["@values", "max_freq", "#2000000", "#2850000", "#3100000"],
        ["@values", "min_freq", "#1600000", "#1800000", "#1800000"]
      ]
    },
    "pedestal": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "pedestal_active"],
        ["$uclamp_heavy", "1.00"]
      ],
      "app": [
        ["@preset", "comm_app"],
        ["@charge", "normal"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["@charge", "suspend"],
        ["$mi_thermal", "10"],
        ["@limiters", "NONE"],
        ["@values", "min_freq", "1400000", "1800000", "1900000"],
        ["@values", "max_freq", "#2000000", "#2850000", "#2850000"],
        ["@values", "min_freq", "#1400000", "#1800000", "#1900000"]
      ]
    }
  },
  "apps": [
    {
      "friendly": "视频/短视频/滚动优化/社交/",
      "categories": ["Video", "ShortVideo", "ScrollOpt", "IM"],
      "import": "_Apps.json"
    },
    {
      "friendly": "极速响应",
      "categories": ["Launcher"],
      "import": "_Launcher.json"
    },
    {
      "friendly": "待机",
      "packages": ["standby"],
      "import": "_Standby.json"
    },
    {
      "friendly": "扫码",
      "categories": ["Scanner"],
      "import": "_Scanner.json"
    },
    {
      "friendly": "相机",
      "categories": ["Camera"],
      "import": "_Camera.json"
    },
    {
      "friendly": "狗屎/毒瘤",
      "categories": ["MiniProgram"],
      "import": "_MiniProgram.json"
    },
    {
      "friendly": "Benchmark",
      "categories": ["CPUBenchmark", "GPUBenchmark"],
      "import": "_Games.json"
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Apps.json"
    }
  ],
  "games": [
    {
      "friendly": "原神",
      "categories": ["GenshinImpact"],
      "import": "_GenshinImpact.json"
    },
    {
      "friendly": "王者荣耀/LOL",
      "packages": ["com.tencent.tmgp.sgame", "com.garena.game.kgtw", "com.tencent.lolm"],
      "import": "_SGAME.json"
    },
    {
      "friendly": "QQ飞车",
      "packages": ["com.tencent.tmgp.speedmobile"],
      "import": "_SpeedMobile.json"
    },
    {
      "friendly": "和平精英",
      "packages": ["com.tencent.tmgp.pubgmhd",  "com.tencent.ig"],
      "import": "_PUBG.json"
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_FAS.json"
    }
  ]
}