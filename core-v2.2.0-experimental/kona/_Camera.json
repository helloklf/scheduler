{
  "friendly": "相机",
  "call": [
    ["@cpuset", "0-1", "0-4", "0-7", "0-7"]
  ],
  "modes": [{
    "mode": ["powersave", "balance"],
    "booster": {
      "events": ["presets"],
      "duration": 3000,
      "idle_delay": 4000,
      "enter": [
        ["@cpu_freq", "policy0", "1500MHz", "max"],
        ["@cpu_freq", "policy4", "1800MHz", "2.5Ghz"],
        ["@cpu_freq", "policy7", "1800MHz", "2.85Ghz"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["@cpu_freq", "policy0", "min", "1800MHz"],
        ["@cpu_freq", "policy4", "min", "2200MHz"],
        ["@cpu_freq", "policy7", "min", "2300MHz"],
        ["@high_rate", "off"]
      ]
    }
  }],
  "booster": {
    "events": ["touch", "buttons"],
    "duration": 2000,
    "idle_delay": 2000,
    "enter": [ ["@high_rate", "on"] ],
    "exit": [ ["@high_rate", "off"] ]
  }
}