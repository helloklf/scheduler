{
  "friendly": "极速响应",
  "packages": [
    "com.miui.home",
    "com.meizu.flyme.launcher",
    "net.oneplus.launcher",
    "com.oppo.launcher",
    "ch.deletescape.lawnchair.ci",
    "com.mi.android.globallauncher",
    "com.teslacoilsw.launcher",
    "com.android.quicksearchbox",
    "net.oneplus.h2launcher",
    "com.oneplus.hydrogen.launcher",
    "com.microsoft.launcher",
    "org.lineageos.trebuchet",
    "org.mokee.lawnchair",
    "com.google.android.apps.nexuslauncher",
    "ch.deletescape.lawnchair.plah"
  ],
  "sensors": [],
  "modes": [{
    "mode": ["powersave"],
    "booster": {
      "events": ["presets"],
      "duration": 3000,
      "idle_delay": 1000,
      "enter": [
        ["$dcs_mode", "0"],
        ["@cpuset", "1-3", "3-4", "1-7", "0-7"],
        ["@preset", "perf_active"],
        ["@cpu_freq", "cpu0", "500000", "1500000"],
        ["@cpu_freq", "cpu4", "600000", "2050000"],
        ["@cpu_freq", "cpu7", "1300000", "2200000"],
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["@classical", "cpu4", "82", "65", "6", "2"],
        ["$max_cpus_cpu7", "1"],
        ["@high_rate", "on"],
        ["$gpu_dvfs_margin", "30"]
      ],
      "exit": [
        ["$dcs_mode", "1"],
        ["@cpuset", "0-4", "0-4", "0-6", "0-7"],
        ["@preset", "perf_idle"],
        ["@cpu_freq", "cpu0", "200000", "1200000"],
        ["@cpu_freq", "cpu4", "400000", "1800000"],
        ["@cpu_freq", "cpu7", "1300000", "1800000"],
        ["$core_ctl_4", "1"],
        ["$core_ctl_7", "1"],
        ["@classical", "cpu4", "90", "80", "2", "1"],
        ["$max_cpus_cpu7", "0"],
        ["@high_rate", "off", "500"]
      ]
    }
  }, {
    "mode": ["balance"],
    "call": [
      ["$core_ctl_4", "1"],
      ["$core_ctl_7", "1"]
    ],
    "booster": {
      "events": ["presets"],
      "duration": 3000,
      "idle_delay": 1000,
      "enter": [
        ["$dcs_mode", "0"],
        ["@cpuset", "0-3", "3-4", "0-7", "0-7"],
        ["@preset", "perf_active"],
        ["@cpu_freq", "cpu0", "500000", "1600000"],
        ["@cpu_freq", "cpu4", "700000", "2200000"],
        ["@cpu_freq", "cpu7", "1300000", "2600000"],
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["@high_rate", "on"],
        ["$gpu_dvfs_margin", "30"]
      ],
      "exit": [
        ["$dcs_mode", "1"],
        ["@cpuset", "0-2", "0-3", "0-6", "0-7"],
        ["@preset", "perf_fg_opt"],
        ["@cpu_freq", "cpu0", "min", "1400000"],
        ["@cpu_freq", "cpu4", "min", "2000000"],
        ["@cpu_freq", "cpu7", "1300000", "2000000"],
        ["$core_ctl_4", "1"],
        ["$core_ctl_7", "1"],
        ["@high_rate", "off", "500"]
      ]
    }
  }, {
    "mode": ["performance", "fast"],
    "booster": {
      "events": ["presets"],
      "duration": 3000,
      "enter": [
        ["$uclamp_bg_min", "max"],
        ["$uclamp_fg_min", "max"],
        ["$uclamp_top_min", "max"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["@high_rate", "off"]
      ]
    }
  }]
}