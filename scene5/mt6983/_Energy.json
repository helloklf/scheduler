      "exit": [
        ["$dcs_mode", "1"],
        ["@cpuset", "1", "0-3", "1-6", "0-6"],
        ["$cpu_min_0", "#200000"], ["$cpu_max_0", "#800000"], ["$cpu_min_4", "#400000"],
        ["$core_ctl_4", "1"], ["$core_ctl_7", "1"],
