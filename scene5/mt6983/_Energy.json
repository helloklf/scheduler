        ["$core_ctl_4", "0"], ["$core_ctl_7", "0"],
        ["@classical", "cpu0", "82", "70", "5", "1"],
        ["@classical", "cpu4", "90", "75", "10", "1"],
        ["$uclamp_bg_max", "max"], ["$uclamp_fg_max", "max"], ["$uclamp_top_max", "max"],
        ["$max_cpus_cpu7", "1"],
        ["@high_rate", "on"]
