    "booster": {
      "events": ["presets"],
      "duration": 2000,
      "idle_delay": 3000,
      "enter": [
        ["@cpuset", "1-3", "", "0-7", ""],
        ["$cpu_min_0", "#800000"],
        ["@classical", "cpu0", "80", "70", "5", "1"],
        ["@classical", "cpu4", "85", "75", "10", "1"],
        ["@preset", "perf_active"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["@cpuset", "1", "", "0-6", ""],
        ["@classical", "cpu0", "98", "92", "2", "1"],
        ["@classical", "cpu4", "99", "95", "2", "1"],
        ["@preset", "perf_idle"],
        ["$cpu_min_0", "#20000"],
        ["@high_rate", "off"]
      ]
    }
  },{
    "mode": ["performance"],
    "booster": {
      "events": ["presets"],
      "duration": 2000,
      "enter": [ ["$uclamp_fg_min", "0.05"], ["$uclamp_top_min", "0.25"] ]
    }
  }, {
    "mode": ["fast"],
    "booster": {
      "events": ["presets"],
      "duration": 2000,
      "enter": [ ["$uclamp_fg_min", "max"], ["$uclamp_top_min", "max"] ]
