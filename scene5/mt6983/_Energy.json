        ["@classical", "cpu4", "99", "95", "2", "1"],
        ["$uclamp_bg_max", "2.00"], ["$uclamp_fg_max", "10.00"], ["$uclamp_top_max", "25.00"],
        ["$max_cpus_cpu7", "0"],
        ["@high_rate", "off"]
      ]
    }
