{
  "platform": "mt6983",
  "platform_name": "D9000",
  "framework": 130,
  "alias": {
    "gpu_fixed": "/proc/gpufreqv2/fix_target_opp_index",
    "gpu_freq": "/sys/kernel/ged/hal/custom_upbound_gpu_freq",
    "boost_affinity": "/sys/module/mtk_fpsgo/parameters/boost_affinity",
    "max_cpus_cpu0": "/sys/devices/system/cpu/cpu0/core_ctl/max_cpus",
    "max_cpus_cpu4": "/sys/devices/system/cpu/cpu4/core_ctl/max_cpus",
    "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
    "uclamp_heavy": "/dev/cpuctl/heavy/cpu.uclamp.min",
    "ddr_freq_min": "/sys/devices/platform/1c00f000.dvfsrc/mtk-dvfsrc-devfreq/devfreq/mtk-dvfsrc-devfreq/min_freq",
    "ddr_freq_max": "/sys/devices/platform/1c00f000.dvfsrc/mtk-dvfsrc-devfreq/devfreq/mtk-dvfsrc-devfreq/max_freq",
    "dcs_mode": "/sys/kernel/ged/hal/dcs_mode",
    "gpu_dvfs_margin": "/sys/kernel/ged/hal/timer_base_dvfs_margin",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig",
    "load_scaling_x": "/sys/module/mtk_fpsgo/parameters/load_scaling_x",
    "pcie_aspm": "/sys/module/pcie_aspm/parameters/policy",
    "ged_kpi": "/sys/module/sspm_v3/holders/ged/parameters/is_GED_KPI_enabled",
    "fpsgo": "/sys/kernel/fpsgo/common/fpsgo_enable",
    "charge_limit": "/sys/class/power_supply/battery/charge_control_limit",
    "night_charging": "/sys/class/power_supply/battery/night_charging",
    "cpufreq": "/proc/cpudvfs/cpufreq_debug",
    "gpu_min": "/sys/devices/platform/soc/13000000.mali/devfreq/13000000.mali/min_freq",
    "gpu_max": "/sys/devices/platform/soc/13000000.mali/devfreq/13000000.mali/max_freq",
    "ged_loading_mode": "/sys/kernel/ged/hal/dvfs_loading_mode",
    "ddr_force_opp": "/sys/devices/platform/1c00f000.dvfsrc/1c00f000.dvfsrc:dvfsrc-helper/dvfsrc_force_vcore_dvfs_opp"
  },
  "features": {
    "high_rate": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_refresh_rate",
      "device_policy": [{
        "device": "matisse", "name": "K50Pro", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0", "mode_standby": "0",
        "1440P60Hz": "0", "1440P120Hz": "1", "1080P120Hz": "2", "1080P60Hz": "3"
      }]
    },
    "processes": [
      {
        "app": "com.miui.home", "group": "top-app", "cpuctl": "heavy",
        "delay": 2000, "interval": 5000, "repeat": 3
      },
      {
        "app": "com.android.systemui", "group": "top-app", "cpuctl": "heavy",
        "delay": 2000, "interval": 5000, "repeat": 0
      }
    ],
    "charge_control": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_charge_control",
      "suspend": [
        ["$charge_limit", "15"],
        ["$night_charging", "1"]
      ],
      "normal": [
        ["$charge_limit", "0"],
        ["$night_charging", "0"]
      ]
    }
  },
  "presets": {
    "reset": [
      ["$cpufreq", "0 200000 1800000"], ["$cpufreq", "4 400000 2850000"], ["$cpufreq", "7 1300000 3200000"],
      ["$max_cpus_cpu0", "4"], ["$max_cpus_cpu4", "3"], ["$max_cpus_cpu7", "1"],
      ["$ddr_force_opp", "999"],
      ["$core_ctl_0", "0"], ["$core_ctl_4", "0"], ["$core_ctl_7", "0"]
    ],
    "kpi_on": [
      ["$ged_kpi", "#1"],
      ["$fpsgo", "#1"]
    ],
    "kpi_off": [
      ["$fpsgo", "#0"],
      ["$ged_kpi", "#0"]
    ],
    "perf_normal": [
      ["@uclamp", "0.00~max", "0.00~max", "0.00~max"]
    ],
    "perf_fg_opt": [
      ["@uclamp", "0.00~1.00", "0.00~max", "0.00~max"]
    ],
    "perf_gaming": [
      ["@uclamp", "0.00~1.00", "0.00~max", "2.00~max"]
    ],
    "perf_active": [
      ["@uclamp", "0.00~max", "0.00~max", "2.00~max"]
    ],
    "perf_boost": [
      ["@uclamp", "0.00~max", "10.00~max", "max~max"]
    ],
    "perf_idle": [
      ["@uclamp", "0.00~0.00", "0.00~15.00", "0.00~25.00"]
    ],
    "perf_doze": [
      ["@uclamp", "0.00~0.00", "0.00~1.00", "0.00~10.00"]
    ],
    "governor_normal": [
      ["@governor", "schedutil"]
    ],
    "governor_durable": [
      ["@classical", "cpu0", "83", "70", "4", "1"],
      ["@classical", "cpu4", "85", "75", "4", "1"],
      ["@classical", "cpu7", "85", "73", "3", "1"]
    ],
    "governor_overheating": [
      ["@classical", "cpu0", "84", "72", "4", "1"],
      ["@classical", "cpu4", "86", "76", "3", "1"],
      ["@classical", "cpu7", "88", "75", "3", "1"]
    ],
    "governor_idle": [
      ["@classical", "cpu0", "99", "95", "2", "1"],
      ["@classical", "cpu4", "99", "95", "2", "1"],
      ["@classical", "cpu7", "90", "78", "3", "1"]
    ]
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@preset", "reset"],
        ["$uclamp_heavy", "0"],
        ["@classical", "cpu0", "78", "67", "3", "1"],
        ["@classical", "cpu4", "78", "68", "3", "1"],
        ["@classical", "cpu7", "78", "68", "3", "1"],
        ["@perf_turbo", "mt6983", "daily", "20"],
        ["$dcs_mode", "0"],
        ["$ddr_freq_min", "#800000000"],
        ["@cpuset", "1-3", "1-3", "1-6", "0-7"],
        ["@cpu_freq", "cpu0", "200000", "1200000"],
        ["@cpu_freq", "cpu4", "400000", "1800000"],
        ["@cpu_freq", "cpu7", "1300000", "1800000"],
        ["@preset", "perf_fg_opt"],
        ["@high_rate", "off", "3000"],
        ["@charge", "normal"]
      ]
    },
    "balance": {
      "call": [
        ["@preset", "reset"],
        ["$uclamp_heavy", "0"],
        ["@governor", "schedutil"],
        ["@perf_turbo", "mt6983", "daily", "15"],
        ["$dcs_mode", "0"],
        ["$ddr_freq_min", "#800000000"],
        ["@cpuset", "1-3", "1-3", "1-6", "0-7"],
        ["@cpu_freq", "cpu0", "200000", "1600000"],
        ["@cpu_freq", "cpu4", "400000", "2300000"],
        ["@cpu_freq", "cpu7", "1300000", "2300000"],
        ["@preset", "perf_fg_opt"],
        ["@high_rate", "on"],
        ["@charge", "normal"]
      ]
    },
    "performance": {
      "call": [
        ["@preset", "reset"],
        ["$uclamp_heavy", "1"],
        ["@governor", "schedutil"],
        ["@perf_turbo", "mt6983", "silent", "0"],
        ["$dcs_mode", "0"],
        ["@cpuset", "1-3", "1-3", "1-6", "0-7"],
        ["@cpu_freq", "cpu0", "400000", "1800000"],
        ["@cpu_freq", "cpu4", "400000", "2650000"],
        ["@cpu_freq", "cpu7", "1300000", "2800000"],
        ["@preset", "perf_normal"],
        ["@high_rate", "on"],
        ["@charge", "normal"]
      ]
    },
    "fast": {
      "call": [
        ["@preset", "reset"],
        ["$uclamp_heavy", "1"],
        ["@governor", "schedutil"],
        ["@perf_turbo", "mt6983", "perf", "0"],
        ["$dcs_mode", "0"],
        ["@cpuset", "1-3", "1-3", "1-6", "0-7"],
        ["@cpu_freq", "cpu0", "1000000", "max"],
        ["@cpu_freq", "cpu4", "1500000", "max"],
        ["@cpu_freq", "cpu7", "1500000", "3000000"],
        ["@preset", "perf_active"],
        ["$mi_thermal", "10"],
        ["@high_rate", "on"],
        ["@charge", "normal"]
      ]
    },
    "pedestal": {
      "call": [
        ["@preset", "reset"],
        ["$uclamp_heavy", "1"],
        ["@governor", "schedutil"],
        ["@perf_turbo", "mt6983", "extreme", "0"],
        ["$dcs_mode", "0"],
        ["$ddr_force_opp", "3"],
        ["@cpuset", "0-2", "3-4", "1-6", "0-7"],
        ["@cpu_freq", "cpu0", "1400000", "max"],
        ["@cpu_freq", "cpu4", "2200000", "max"],
        ["@cpu_freq", "cpu7", "2000000", "max"],
        ["@preset", "perf_active"],
        ["$mi_thermal", "10"],
        ["@high_rate", "on"],
        ["@charge", "suspend"]
      ]
    }
  },
