    "mode": ["performance"],
    "booster": {
      "events": ["presets"],
      "duration": 2000,
      "enter": [
        ["$uclamp_fg_min", "0.05"],
        ["$uclamp_top_min", "0.25"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["@high_rate", "off"]
      ]
    }
  }, {
    "mode": ["fast"],
    "booster": {
      "events": ["presets"],
      "duration": 2000,
      "enter": [
        ["$uclamp_fg_min", "max"],
        ["$uclamp_top_min", "max"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["@high_rate", "off"]
      ]
