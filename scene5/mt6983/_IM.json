        ["$cpu_min_0", "#500000"],
        ["$cpu_min_4", "#700000"],
        ["$dcs_mode", "0"],
        ["@preset", "perf_fg_opt"],
        ["$core_ctl_7", "0"],
        ["$max_cpus_cpu7", "1"],
        ["@high_rate", "on"],
        ["@cpuset", "1-3", "1-3", "1-7", "0-7"],
