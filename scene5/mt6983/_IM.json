        ["@classical", "cpu4", "88", "80", "2", "1"]
      ],
      "exit": [
        ["$cpu_min_0", "#200000"],
        ["$cpu_min_4", "#400000"],
        ["@cpuset", "1", "1-3", "1-4", "0-7"],
        ["$core_ctl_0", "0"], ["$core_ctl_4", "0"], ["$core_ctl_7", "1"],
