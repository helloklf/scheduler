    "call": [
      ["$boost_affinity", "#0"]
    ],
    "booster": {
      "events": ["presets"],
      "duration": 2000,
      "idle_delay": 3000,
      "enter": [
        ["$core_ctl_7", "0"],
        ["@cpuset", "1-3", "1-3", "1-7", "0-7"],
        ["$cpu_min_0", "#500000"],
        ["$cpu_min_4", "#700000"],
        ["@preset", "perf_fg_opt"],
        ["$dcs_mode", "0"], ["@high_rate", "on"],
        ["@classical", "cpu0", "80", "70", "4", "1"],
        ["@classical", "cpu4", "87", "73", "4", "1"],
        ["@classical", "cpu7", "87", "73", "4", "1"]
