{
  "friendly": "LOL Mobile",
  "modes": [{
    "mode": ["powersave"],
    "call": [
      ["@limiters", "g1:cpu0", "g1:cpu4", "g1:cpu7"],
      ["@gpu_freq", "min", "315000"]
    ],
    "sensors": [
      {
        "sensor": "fps",
        "interval": 3000,
        "rules": [
          { "threshold": [-1, 100], "enter_once": [["@preset", "bw_normal"]] },
          { "threshold": [99,  75], "enter_once": [["@preset", "bw_low"]] },
          { "threshold": [70,  -1], "enter_once": [["@preset", "bw_low"]] }
        ]
      }
    ]
  },{
    "mode": ["balance"],
    "call": [
      ["@limiters", "g2:cpu0", "g2:cpu4", "g2:cpu7"],
      ["@gpu_freq", "min", "491000"],
