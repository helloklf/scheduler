{
  "friendly": "狗屎/毒瘤",
  "packages": [
    "com.tencent.mm:appbrand",
    "com.tencent.mm:toolsmp"
  ],
  "modes": [{
    "mode": ["powersave", "balance"],
    "call": [
      ["$offline_cpu0", "0"],
      ["$offline_cpu4", "0"],
      ["$offline_cpu7", "0"],
      ["@cpuset", "0", "0-4", "0-7", "0-7"],
      ["@cpu_freq", "policy0", "1.4Ghz", "max"],
      ["@cpu_freq", "policy4", "1Ghz", "2.4Ghz"],
      ["@cpu_freq", "policy7", "min", "2.4Ghz"],
      ["$mi_thermal", "10"]
    ]
  }]
}