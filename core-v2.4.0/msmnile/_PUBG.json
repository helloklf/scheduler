{
  "friendly": "和平精英",
  "packages": [
    "com.tencent.tmgp.pubgmhd",
    "com.tencent.ig"
  ],
  "cpuset": {
    "interval": 1000,
    "comm": {
      "7": ["RenderThread"],
      "4-6": ["Thread-", "TaskGraphNP"]
    },
    "other": "0-6"
  },
  "modes": [
    {
      "mode": ["powersave"],
      "logger": false,
      "cpuset": {
        "interval": 1000,
        "comm": {
          "4-7": ["RenderThread"],
          "4-6": ["Thread-", "TaskGraphNP"]
        },
        "other": "0-6"
      }
    },
    {
      "mode": ["performance"],
      "logger": false,
      "booster": {
        "duration": 2000,
        "enter": [
          ["$stune_top_boost", "10"]
        ],
        "exit": [
          ["$sched_boost", "0"]
        ]
      }
    },
    {
      "mode": ["fast"],
      "logger": false,
      "booster": {
        "duration": 2000,
        "enter": [
          ["$stune_top_boost", "60"]
        ],
        "exit": [
          ["$sched_boost", "0"]
        ]
      }
    }
  ]
}