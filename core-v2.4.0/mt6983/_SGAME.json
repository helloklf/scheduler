{
  "friendly": "王者荣耀CN",
  "cpuset": {
    "interval": 1000,
    "heavy_thread": "Thread-",
    "heavy_cores": "6",
    "comm": {
      "7": ["UnityMain"],
      "4-7": ["UnityPreload"],
      "6": ["UnityGfx"],
      "4-5": ["CoreThread", "Apollo", "Worker Thread", "NativeThread"],
      "0-5": ["mali-"]
    },
    "other": "0-5"
  },
  "call": [
    ["$boost_affinity", "#0"]
  ],
  "modes": [{
    "mode": ["powersave"],
    "call": [
      ["@limiters", "g1:cpu0", "g1:cpu4", "g1:cpu7"],
      ["$fpsgo", "#0"]
    ],
    "sensors": [{
      "sensor": "fps",
      "interval": 10000,
      "rules": [
        { "threshold": [-1, 100], "enter_once": [["@mtk_feas", "12", "", "120"]] },
        { "threshold": [99,  75], "enter_once": [["@mtk_feas", "15", "", "90"]] },
        { "threshold": [70,  -1], "enter_once": [["@mtk_feas", "18", "", "60"]] }
      ]
    }]
  }, {
    "mode": ["balance"],
    "call": [
      ["@limiters", "g2:cpu0", "g2:cpu4", "g3:cpu7"],
      ["$fpsgo", "#0"]
    ],
    "sensors": [{
      "sensor": "fps",
      "interval": 5000,
      "rules": [
        { "threshold": [-1, 100], "enter_once": [["@mtk_feas", "8", "", "120"]] },
        { "threshold": [99,  75], "enter_once": [["@mtk_feas", "11", "", "90"]] },
        { "threshold": [70,  -1], "enter_once": [["@mtk_feas", "15", "", "60"]] }
      ]
    }]
  }, {
    "mode": ["performance"],
    "call": [
      ["@limiters", "g3:cpu0", "g3:cpu4", "g3:cpu7"],
      ["$fpsgo", "0"]
    ],
    "sensors": [{
      "sensor": "fps",
      "interval": 5000,
      "rules": [
        { "threshold": [-1, 100], "enter_once": [["@mtk_feas", "4", "", "120"]] },
        { "threshold": [99, 75], "enter_once": [["@mtk_feas", "9", "", "90"]] },
        { "threshold": [70, -1], "enter_once": [["@mtk_feas", "14", "", "60"]] }
      ]
    }]
  }, {
    "mode": ["*"],
    "call": [
      ["@limiters", "g4:cpu0", "g4:cpu4", "g4:cpu7"],
      ["$fpsgo", "0"]
    ]
  }]
}