{
  "platform": "sm6150",
  "platform_name": "730G",
  "framework": 210,
  "alias": {
    "gpu_max_khz": "/sys/class/kgsl/kgsl-3d0/devfreq/max_freq",
    "max_cpus_cpu6": "/sys/devices/system/cpu/cpu6/core_ctl/max_cpus",
    "min_cpus_cpu6": "/sys/devices/system/cpu/cpu6/core_ctl/min_cpus",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig",
    "msm_min_freq": "/sys/module/msm_performance/parameters/cpu_min_freq",
    "msm_max_freq": "/sys/module/msm_performance/parameters/cpu_max_freq",
    "llcc_ddr_max": "/sys/class/devfreq/soc:qcom,cpu-llcc-ddr-bw/max_freq",
    "cpu_llcc_max": "/sys/class/devfreq/soc:qcom,cpu-cpu-llcc-bw/max_freq",
    "llcc_ddr_min": "/sys/class/devfreq/soc:qcom,cpu-llcc-ddr-bw/min_freq",
    "cpu_llcc_min": "/sys/class/devfreq/soc:qcom,cpu-cpu-llcc-bw/min_freq"
  },
  "features": {
    "limiter": {
      "active_interval": 20,
      "inactive_interval": 100,
      "logger": false,
      "params": [
        { "id": "p1:cpu0", "max": 1804800, "turning": 1612800, "min": 768000, "margin": 450, "mode": "upper" },
        { "id": "p1:cpu6", "max": 2208000, "turning": 1708800, "min": 979200, "margin": 450, "mode": "upper" },

        { "id": "p2:cpu0", "max": 1804800, "turning": 1612800, "min": 1248000, "margin": 500, "mode": "upper" },
        { "id": "p2:cpu6", "max": 2208000, "turning": 1939200, "min": 1708800, "margin": 550, "mode": "upper" },

        { "id": "p3:cpu0", "max": 1804800, "turning": 1612800, "min": 1017600, "margin": 550, "mode": "bottom" },
        { "id": "p3:cpu6", "min": 2208000, "margin": 550, "mode": "bottom" },

        { "id": "p4:cpu0", "max": 1804800, "turning": 1612800, "min": 1248000, "margin": 600, "mode": "bottom" },
        { "id": "p4:cpu6", "min": 2208000, "margin": 550, "mode": "bottom" },


        { "id": "g1:cpu6", "max": 1900800, "min": 1478400, "margin": 350, "smoothness": 6 },

        { "id": "g2:cpu6", "max": 2208000, "turning": 1900800, "min": 1478400, "margin": 400, "smoothness": 6 },

        { "id": "g3:cpu6", "max": 1996800, "min": 1094400, "margin": 350, "mode": "BOTTOM", "smoothness": 6 },

        { "id": "g4:cpu6", "max": 0, "min": 1478400, "margin": 420, "mode": "BOTTOM", "smoothness": 6  },


        { "id": "idle:cpu0", "max": 1612800, "min": 576000, "margin": 100 },
        { "id": "idle:cpu6", "max": 1939200, "min": 300000, "margin": 100 }
      ]
    }
  },
  "presets": {
    "reset": [
      ["@msm_reset"],
      ["@governor", "schedutil"],
      ["@sched_limit", "0 0", "0 0"]
    ],
    "hispeed_freq": [
      ["/sys/devices/system/cpu/cpufreq/policy0/schedutil/hispeed_freq"],
      ["/sys/devices/system/cpu/cpufreq/policy6/schedutil/hispeed_freq"]
    ],
    "hispeed_load": [
      ["/sys/devices/system/cpu/cpufreq/policy0/schedutil/hispeed_load"],
      ["/sys/devices/system/cpu/cpufreq/policy6/schedutil/hispeed_load"]
    ],
    "sched_config": [
      ["/proc/sys/kernel/sched_downmigrate"],
      ["/proc/sys/kernel/sched_upmigrate"],
      ["/proc/sys/kernel/sched_group_downmigrate"],
      ["/proc/sys/kernel/sched_group_upmigrate"]
    ],
    "sched_boost": [
      ["/proc/sys/kernel/sched_boost_top_app"],
      ["/proc/sys/kernel/sched_boost"],
      ["/dev/stune/top-app/schedtune.prefer_idle"],
      ["/dev/stune/top-app/schedtune.boost"]
    ],
    "min_freq": [
      ["$cpu_min_0"], ["$cpu_min_6"]
    ],
    "max_freq": [
      ["$cpu_max_0"], ["$cpu_max_6"]
    ],
    "common_actvie": [
      ["$cpu_max_0", "1804800"],
      ["@preset", "cores_force_on"],
      ["@cpuset", "0-3", "0-3", "0-5", "0-7"]
    ],
    "common_inactvie": [
      ["@cpuset", "0-3", "0-3", "0-5", "0-7"]
    ],
    "common_gaming": [
      ["@limiters", "NONE"],
      ["@cpuset", "0-3", "0-3", "0-5", "0-7"]
    ],

    "cores_force_on": [
      ["$core_ctl_6", "0"], ["$max_cpus_cpu6", "2"], ["$min_cpus_cpu6", "2"]
    ],
    "cores_force_off": [
      ["$core_ctl_6", "0"], ["$min_cpus_cpu6", "0"], ["$max_cpus_cpu6", "0"]
    ],
    "cores_auto_off": [
      ["$core_ctl_6", "1"], ["$min_cpus_cpu6", "0"], ["$max_cpus_cpu6", "2"]
    ],
    "bw_max": [
      ["$llcc_ddr_max", "#6881"],
      ["$cpu_llcc_max", "#14236"],
      ["$llcc_ddr_min", "#762"],
      ["$cpu_llcc_min", "#4577"]
    ],
    "bw_low": [
      ["$llcc_ddr_max", "#3879"],
      ["$cpu_llcc_max", "#9155"],
      ["$llcc_ddr_min", "#762"],
      ["$cpu_llcc_min", "#4577"]
    ],

    "powersave_active": [
      ["@preset", "common_actvie"],
      ["@limiters", "p1:cpu0", "p1:cpu6"],
      ["@values", "min_freq", "1094400", "844800"],
      ["@repeat", "sched_config", "70", "80", "150", "400"]
    ],
    "powersave_inactive": [
      ["@limiters", "idle:cpu0", "idle:cpu6"],
      ["@values", "min_freq", "300000", "710400"],
      ["@preset", "cores_auto_off"],
      ["@repeat", "sched_config", "85", "95", "150", "400"]
    ],
    "powersave_idle": [
      ["@limiters", "idle:cpu0", "idle:cpu6"],
      ["@preset", "common_inactvie"],
      ["@values", "min_freq", "300000", "710400"],
      ["@preset", "cores_auto_off"],
      ["@repeat", "sched_config", "85", "95", "150", "400"]
    ],

    "balance_active": [
      ["@preset", "common_actvie"],
      ["@limiters", "p2:cpu0", "p2:cpu6"],
      ["@values", "min_freq", "1209600", "960000"],
      ["@repeat", "sched_config", "65", "75", "150", "400"]
    ],
    "balance_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "common_inactvie"],
      ["@values", "min_freq", "300000", "710400"],
      ["@repeat", "sched_config", "85", "95", "150", "400"]
    ],

    "performance_active": [
      ["@preset", "common_actvie"],
      ["@limiters", "p3:cpu0", "p3:cpu6"],
      ["@values", "min_freq", "1401600", "1075200"],
      ["@repeat", "sched_config", "62", "70", "150", "400"]
    ],
    "performance_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "common_inactvie"],
      ["@values", "min_freq", "1209600", "710400"],
      ["@repeat", "sched_config", "85", "95", "150", "400"]
    ],

    "fast_active": [
      ["@preset", "common_actvie"],
      ["@limiters", "p4:cpu0", "p4:cpu6"],
      ["@values", "min_freq", "1497600", "1440000"],
      ["@repeat", "sched_config", "62 62", "70 78", "300", "400"]
    ],
    "fast_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "common_inactvie"],
      ["@values", "min_freq", "1209600", "710400"],
      ["@repeat", "sched_config", "85", "95", "150", "400"]
    ]
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "powersave_active"],
        ["@values", "hispeed_freq", "1248000", "806400"],
        ["@values", "hispeed_load", "90", "90"],
        ["@values", "sched_boost", "0", "0", "0", "0"],
        ["@gpu_freq", "min", "500000"],
        ["@preset", "bw_low"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["@cpu_freq", "cpu0", "300000", "1804800"],
        ["@cpu_freq", "cpu6", "300000", "1939200"]
      ]
    },
    "balance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "balance_active"],
        ["@values", "hispeed_freq", "1248000", "1209600"],
        ["@values", "hispeed_load", "90", "90"],
        ["@values", "sched_boost", "1", "0", "0", "0"],
        ["@gpu_freq", "min", "630000"],
        ["@preset", "bw_max"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["@cpu_freq", "cpu0", "300000", "1804800"],
        ["@cpu_freq", "cpu6", "300000", "1939200"]
      ]
    },
    "performance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "performance_active"],
        ["@sched_limit", "0 0", "0 0", "0 0"],
        ["@values", "hispeed_freq", "1708800", "1209600"],
        ["@values", "hispeed_load", "87", "87"],
        ["@values", "sched_boost", "1", "0", "0", "0"],
        ["@gpu_freq", "min", "730000"],
        ["@preset", "bw_max"]
      ],
      "app": [],
      "game": [
        ["@preset", "common_gaming"],
        ["@cpu_freq", "cpu0", "300000", "1804800"],
        ["@cpu_freq", "cpu6", "300000", "2208000"],
        ["$mi_thermal", "13"]
      ]
    },
    "fast": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "fast_active"],
        ["@values", "hispeed_freq", "0", "0"],
        ["@values", "hispeed_load", "0", "0"],
        ["@values", "sched_boost", "1", "0", "1", "10"],
        ["@gpu_freq", "min", "max"],
        ["@preset", "bw_max"]
      ],
      "app": [],
      "game": [
        ["@preset", "common_gaming"],
        ["@cpu_freq", "cpu0", "1708800", "1804800"],
        ["@cpu_freq", "cpu6", "1939200", "2208000"],
        ["$mi_thermal", "13"]
      ]
    }
  },
  "apps": [
    {
      "friendly": "极速响应",
      "categories": ["Launcher"],
      "import": "_Launcher.json"
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Apps.json"
    }
  ],
  "games": [
    {
      "friendly": "王者荣耀",
      "packages": ["com.tencent.tmgp.sgame"],
      "cpuset": {
        "comm": {
          "6-7": ["UnityGfx", "UnityMain", "UnityPreload", "Thread-"],
          "3-5": ["Worker Thread", "NativeThread", "Audio", "NDK Media", "GVoice", "FMOD mixer", "FMOD stream", "ff_read"]
        },
        "other": "0-7"
      },
      "modes": [
        {
          "mode": ["powersave"],
          "call": [["@limiters", "g1:cpu6"]]
        },
        {
          "mode": ["balance"],
          "call": []
        },
        {
          "mode": ["performance"],
          "call": []
        },
        {
          "mode": ["fast"],
          "call": [["@limiters", "g4:cpu6"]]
        }
      ]
    },
    {
      "friendly": "和平精英",
      "packages": ["com.tencent.tmgp.pubgmhd", "com.tencent.tmgp.pubgmhdce",  "com.tencent.ig"],
      "cpuset": {
        "interval": 1000,
        "comm": {
          "7": ["RenderThread"],
          "4-6": ["Thread-", "TaskGraphNP"]
        },
        "other": "0-5"
      },
      "modes": [
        {
          "mode": ["fast"],
          "booster": {
            "duration": 2000,
            "enter": [
              ["$stune_top_boost", "60"]
            ],
            "exit": [
              ["$sched_boost", "0"]
            ]
          }
        }
      ]
    },
    {
      "friendly": "All",
      "packages": ["*"]
    }
  ]
}