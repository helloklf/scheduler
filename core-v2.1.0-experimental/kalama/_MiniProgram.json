{
  "friendly": "狗屎/毒瘤",
  "packages": [
    "com.tencent.mm:appbrand",
    "com.tencent.mm:toolsmp"
  ],
  "booster": {
    "events": ["presets"],
    "duration": 2000,
    "idle_delay": 2000,
    "enter": [ ["@high_rate", "on"] ],
    "exit": [ ["@high_rate", "off"] ]
  },
  "modes": [{
    "mode": ["powersave", "balance"],
    "call": [
      ["@uclamp", "0.00~max", "0.00~max", "0.00~max"],
      ["@cpuset", "1", "0-4", "0-7", "0-7"],
      ["@cpu_freq", "policy0", "1056000", "1555200"],
      ["@cpu_freq", "policy3", "499200", "2342400"],
      ["@cpu_freq", "policy7", "595200", "2476800"],
      ["$mi_thermal", "10"]
    ]
  }]
}