{
    "platform": "pineapple",
    "platform_name": "8GEN3",
    "framework": 220,
    "alias": {
      "gpu_max_khz": "/sys/class/kgsl/kgsl-3d0/devfreq/max_freq",
      "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
      "min_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/min_cpus",
      "perfmgr_enable": "/sys/module/perfmgr/parameters/perfmgr_enable",
      "cpu_min_2": "/sys/devices/system/cpu/cpufreq/policy2/scaling_min_freq",
      "cpu_max_2": "/sys/devices/system/cpu/cpufreq/policy2/scaling_max_freq",
      "cpu_min_5": "/sys/devices/system/cpu/cpufreq/policy5/scaling_min_freq",
      "cpu_max_5": "/sys/devices/system/cpu/cpufreq/policy5/scaling_max_freq"
    },
    "features": {
      "limiter": {
        "active_interval": 30,
        "gaming_interval": 30,
        "inactive_interval": 100,
        "logger": false,
        "params": [
          { "id": "p1:cpu0", "step": -0.9, "max": 1248000, "min": 672000, "margin": 100 },
          { "id": "p1:cpu2", "step": -0.9, "max": 1824000, "min": 729600, "margin": 150 },
          { "id": "p1:cpu5", "step": -0.9, "max": 2054400, "min": 844800, "margin": 200 },
          { "id": "p1:cpu7", "step": -0.8, "max": 2092800, "min": 864000, "margin": 220 },
  
          { "id": "p2:cpu0", "step": -0.9, "max": 1459200, "min": 672000, "margin": 120 },
          { "id": "p2:cpu2", "step": -0.9, "max": 2188800, "min": 1612800, "margin": 150 },
          { "id": "p2:cpu5", "step": -0.7, "max": 2457600, "min": 1785600, "margin": 300 },
          { "id": "p2:cpu7", "step": -0.7, "max": 2476800, "min": 1843200, "margin": 300 },
  
          { "id": "p3:cpu0", "step": -0.9, "max": 1670400, "min": 787200, "margin": 120 },
          { "id": "p3:cpu2", "step": -0.7, "max": 2515200, "min": 1824000, "margin": 150 },
          { "id": "p3:cpu5", "max": 2592000, "min": 2323200, "margin": 300 },
          { "id": "p3:cpu7", "max": 2726400, "min": 2342400, "margin": 300 },
  
          { "id": "p4:cpu0", "step": -0.9, "max": 1804800, "min": 902400, "margin": 150 },
          { "id": "p4:cpu2", "step": -0.7, "max": 2630400, "min": 2131200, "margin": 200 },
          { "id": "p4:cpu5", "max": 2745600, "min": 2592000, "margin": 300 },
          { "id": "p4:cpu7", "max": 2995200, "min": 2726400, "margin": 300 },
  
          { "id": "inactive:cpu0", "max": 1017600, "min": 672000, "margin": 100 },
          { "id": "inactive:cpu2", "max": 1708800, "min": 614400, "margin": 120 },
          { "id": "inactive:cpu5", "max": 1708800, "min": 614400, "margin": 120 },
          { "id": "inactive:cpu7", "max": 1593600, "min": 672000, "margin": 120 },
  
          { "id": "idle:cpu0", "max": 1017600, "min": 672000, "margin": 90 },
          { "id": "idle:cpu2", "max": 1536000, "min": 614400, "margin": 90 },
          { "id": "idle:cpu5", "max": 1536000, "min": 614400, "margin": 90 },
          { "id": "idle:cpu7", "max": 1593600, "min": 672000, "margin": 90 }
        ]
      }
    },
    "presets": {
      "reset": [
        ["@msm_reset"],
        ["@governor", "walt"]
      ],
  
      "hispeed_freq": [
        ["/sys/devices/system/cpu/cpufreq/policy0/walt/hispeed_freq"],
        ["/sys/devices/system/cpu/cpufreq/policy2/walt/hispeed_freq"],
        ["/sys/devices/system/cpu/cpufreq/policy5/walt/hispeed_freq"],
        ["/sys/devices/system/cpu/cpufreq/policy7/walt/hispeed_freq"]
      ],
      "hispeed_load": [
        ["/sys/devices/system/cpu/cpufreq/policy0/walt/hispeed_load"],
        ["/sys/devices/system/cpu/cpufreq/policy2/walt/hispeed_load"],
        ["/sys/devices/system/cpu/cpufreq/policy5/walt/hispeed_load"],
        ["/sys/devices/system/cpu/cpufreq/policy7/walt/hispeed_load"]
      ],
      "latency": [
        ["/sys/devices/system/cpu/cpufreq/policy0/walt/down_rate_limit_us"],
        ["/sys/devices/system/cpu/cpufreq/policy0/walt/up_rate_limit_us"],
        ["/sys/devices/system/cpu/cpufreq/policy2/walt/down_rate_limit_us"],
        ["/sys/devices/system/cpu/cpufreq/policy2/walt/up_rate_limit_us"],
        ["/sys/devices/system/cpu/cpufreq/policy5/walt/down_rate_limit_us"],
        ["/sys/devices/system/cpu/cpufreq/policy5/walt/up_rate_limit_us"],
        ["/sys/devices/system/cpu/cpufreq/policy7/walt/down_rate_limit_us"],
        ["/sys/devices/system/cpu/cpufreq/policy7/walt/up_rate_limit_us"]
      ],
      "min_freq": [
        ["$cpu_min_0"], ["$cpu_min_2"], ["$cpu_min_5"], ["$cpu_min_7"]
      ],
      "max_freq": [
        ["$cpu_max_0"], ["$cpu_max_2"], ["$cpu_max_5"], ["$cpu_max_7"]
      ],
      "cpu7_on": [
        ["$core_ctl_7", "1"],
        ["$max_cpus_cpu7", "#1"],
        ["$min_cpus_cpu7", "#1"]
      ],
      "cpu7_off": [
        ["$core_ctl_7", "1"],
        ["$min_cpus_cpu7", "#0"],
        ["$max_cpus_cpu7", "#0"]
      ],
      "cpu7_auto": [
        ["$core_ctl_7", "1"],
        ["$min_cpus_cpu7", "#0"],
        ["$max_cpus_cpu7", "#1"]
      ],
      "common_active": [
        ["@preset", "cpu7_on"]
      ],
      "common_inactive": [
        ["@values", "min_freq", "307200", "480000", "480000", "595200"]
      ],
      "common_app": [
        ["$perfmgr_enable", "0"],
        ["@cpuset", "2-4", "2-6", "2-7", "2-7"],
        ["/dev/cpuset/top-app/sf/cpus", "2-6"]
      ],
      "common_gaming": [
        ["@limiters", "NONE"],
        ["@cpuset", "0-1", "0-4", "0-4", "2-7"],
        ["/dev/cpuset/top-app/sf/cpus", "2-5"]
      ],
  
      "l3_max": [
        ["/sys/devices/system/cpu/bus_dcvs/L3/soc:qcom,memlat:l3:gold/max_freq", "1804800"],
        ["/sys/devices/system/cpu/bus_dcvs/L3/soc:qcom,memlat:l3:prime/max_freq", "1804800"],
        ["/sys/devices/system/cpu/bus_dcvs/L3/soc:qcom,memlat:l3:prime-compute/max_freq", "1804800"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:l3:silver/max_freq", "1804800"]
      ],
  
      "l3_low": [
        ["/sys/devices/system/cpu/bus_dcvs/L3/soc:qcom,memlat:l3:gold/max_freq", "1574400"],
        ["/sys/devices/system/cpu/bus_dcvs/L3/soc:qcom,memlat:l3:prime/max_freq", "1574400"],
        ["/sys/devices/system/cpu/bus_dcvs/L3/soc:qcom,memlat:l3:prime-compute/max_freq", "1574400"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:l3:silver/max_freq", "1574400"]
      ],
  
      "ddr_max": [
        ["/sys/devices/system/cpu/bus_dcvs/DDR/24091000.qcom,bwmon-ddr/max_freq", "2736000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:gold-compute/max_freq", "1555000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:gold/max_freq", "4224000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:prime-latfloor/max_freq", "4224000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:prime/max_freq", "4224000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:silver/max_freq", "1555000"]
      ],
  
      "ddr_low": [
        ["/sys/devices/system/cpu/bus_dcvs/DDR/24091000.qcom,bwmon-ddr/max_freq", "2736000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:gold-compute/max_freq", "1555000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:gold/max_freq", "2736000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:prime-latfloor/max_freq", "2736000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:prime/max_freq", "2736000"],
        ["/sys/devices/system/cpu/bus_dcvs/DDR/soc:qcom,memlat:ddr:silver/max_freq", "1555000"]
      ],

      "powersave_active": [
        ["@preset", "common_active"],
        ["@limiters", "p1:cpu0", "p1:cpu2", "p1:cpu5", "p1:cpu7"],
        ["@values", "hispeed_freq", "0", "844800", "1075200", "1017600"],
        ["@values", "hispeed_load", "85", "85", "85", "85"]
      ],
      "powersave_inactive": [
        ["@preset", "common_inactive"],
        ["@preset", "cpu7_off"],
        ["@limiters", "idle:cpu0", "idle:cpu2", "idle:cpu5", "idle:cpu7"],
        ["@values", "hispeed_freq", "0", "0", "0", "0"],
        ["@values", "hispeed_load", "90", "90", "90", "90"]
      ],

      "balance_active": [
        ["@preset", "common_active"],
        ["@limiters", "p2:cpu0", "p2:cpu2", "p2:cpu5", "p2:cpu7"],
        ["@values", "hispeed_freq", "0", "1075200", "1286400", "1248000"],
        ["@values", "hispeed_load", "90", "90", "90", "90"]
      ],
      "balance_inactive": [
        ["@preset", "common_inactive"],
        ["@preset", "cpu7_auto"],
        ["@limiters", "inactive:cpu0", "inactive:cpu2", "inactive:cpu5", "inactive:cpu7"],
        ["@values", "hispeed_freq", "0", "729600", "729600", "787200"],
        ["@values", "hispeed_load", "90", "90", "90", "90"]
      ],

      "performance_active": [
        ["@preset", "common_active"],
        ["@limiters", "p3:cpu0", "p3:cpu2", "p3:cpu5", "p3:cpu7"],
        ["@values", "hispeed_freq", "0", "1075200", "1286400", "1248000"],
        ["@values", "hispeed_load", "82", "82", "82", "82"]
      ],
      "performance_inactive": [
        ["@preset", "common_inactive"],
        ["@limiters", "p2:cpu0", "p2:cpu2", "p2:cpu5", "p2:cpu7"],
        ["@values", "hispeed_freq", "0", "960000", "1075200", "1017600"],
        ["@values", "hispeed_load", "82", "82", "82", "82"]
      ],

      "fast_active": [
        ["@preset", "common_active"],
        ["@limiters", "p4:cpu0", "p4:cpu2", "p4:cpu5", "p4:cpu7"],
        ["@values", "hispeed_freq", "0", "1286400", "1497600", "1478400"],
        ["@values", "hispeed_load", "82", "82", "82", "82"]
      ],
      "fast_inactive": [
        ["@preset", "common_inactive"],
        ["@limiters", "p3:cpu0", "p3:cpu2", "p3:cpu5", "p3:cpu7"],
        ["@values", "hispeed_freq", "0", "1075200", "1286400", "1248000"],
        ["@values", "hispeed_load", "82", "82", "82", "82"]
      ],
      "freq_low": [
        ["@values", "max_freq", "1670400", "#1651200", "#1824000", "#1708800"],
        ["@values", "min_freq", "307200", "480000", "480000", "595200"]
      ],
      "freq_middle": [
        ["@values", "max_freq", "1670400", "#1824000", "#2054400", "#2092800"],
        ["@values", "min_freq", "441600", "614400", "614400", "729600"]
      ],
      "freq_high": [
        ["@values", "max_freq", "1804800", "#2246400", "#2457600", "#2476800"],
        ["@values", "min_freq", "672000", "729600", "729600", "864000"]
      ],
      "freq_extreme": [
        ["@values", "max_freq", "#1804800", "#2630400", "#2841600", "#2995200"],
        ["@values", "min_freq", "1056000", "1209600", "1209600", "1171200"]
      ]
    },
    "schemes": {
      "powersave": {
        "call": [
          ["@preset", "reset"],
          ["@preset", "powersave_active"],
          ["@preset", "ddr_low"]
        ],
        "app": [
          ["@preset", "common_app"]
        ],
        "game": [
          ["@preset", "common_gaming"],
          ["@values", "hispeed_freq", "0", "0", "0", "0"],
          ["@preset", "freq_low"]
        ]
      },
      "balance": {
        "call": [
          ["@preset", "reset"],
          ["@preset", "balance_active"],
          ["@preset", "ddr_low"]
        ],
        "app": [
          ["@preset", "common_app"]
        ],
        "game": [
          ["@preset", "common_gaming"],
          ["@values", "hispeed_freq", "0", "960000", "1113600", "1171200"],
          ["@preset", "freq_middle"]
        ]
      },
      "performance": {
        "call": [
          ["@preset", "reset"],
          ["@preset", "performance_active"],
          ["@preset", "ddr_max"]
        ],
        "app": [
          ["@preset", "common_app"]
        ],
        "game": [
          ["@preset", "common_gaming"],
          ["@preset", "freq_high"]
        ]
      },
      "fast": {
        "call": [
          ["@preset", "reset"],
          ["@preset", "fast_active"],
          ["@preset", "ddr_max"]
        ],
        "app": [
          ["@preset", "common_app"]
        ],
        "game": [
          ["$perfmgr_enable", "#0"],
          ["@preset", "common_gaming"],
          ["@preset", "freq_extreme"]
        ]
      }
    },
    "apps": [
      { "friendly": "极速响应", "categories": ["Launcher"], "import": "_Launcher.json" },
      { "friendly": "扫码", "categories": ["Scanner"], "import": "_Scanner.json" },
      { "friendly": "相机", "categories": ["Camera"], "import": "_Camera.json" },
      { "friendly": "狗屎/毒瘤", "categories": ["MiniProgram"], "import": "_MiniProgram.json" },
      { "friendly": "待机", "packages": ["standby"], "import": "_Standby.json" },
      { "friendly": "All", "packages": ["*"], "import": "_Apps.json" }
    ],
    "games": [
      {
        "friendly": "原神",
        "categories": ["GenshinImpact"],
        "import": "_GenshinImpact.json"
      },
      {
        "friendly": "All",
        "packages": ["*"],
        "import": "_FAS.json"
      }
    ]
  }