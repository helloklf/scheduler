{
  "platform": "taro",
  "platform_name": "8+GEN1",
  "alias": {
    "gpu_max_khz": "/sys/class/kgsl/kgsl-3d0/devfreq/max_freq",
    "mod_percent": "/sys/class/kgsl/kgsl-3d0/devfreq/mod_percent",
    "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
    "min_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/min_cpus",
    "bus_ddr_min": "/sys/devices/system/cpu/bus_dcvs/DDR/19091000.qcom,bwmon-ddr/min_freq",
    "bus_ddr": "/sys/devices/system/cpu/bus_dcvs/DDR/19091000.qcom,bwmon-ddr/max_freq",
    "l3_boost": "/sys/devices/system/cpu/bus_dcvs/L3/boost_freq",
    "perfmgr_enable": "/sys/module/perfmgr/parameters/perfmgr_enable",
    "sched_boost": "/proc/sys/walt/sched_boost"
  },
  "features": {
    "gesture_boost": {
      "enable": false,
      "enter": [
        ["$sched_boost", "2"]
      ],
      "exit": [
        ["$sched_boost", "0"]
      ]
    }
  },
  "presets": {
    "reset": [
      ["@msm_reset"],
      ["$l3_boost", "0"],
      ["@governor", "walt"],
      ["$perfmgr_enable", "#0"]
    ],

    "hispeed_freq": [
      ["/sys/devices/system/cpu/cpufreq/policy0/walt/hispeed_freq"],
      ["/sys/devices/system/cpu/cpufreq/policy4/walt/hispeed_freq"],
      ["/sys/devices/system/cpu/cpufreq/policy7/walt/hispeed_freq"]
    ],
    "hispeed_load": [
      ["/sys/devices/system/cpu/cpufreq/policy0/walt/hispeed_load"],
      ["/sys/devices/system/cpu/cpufreq/policy4/walt/hispeed_load"],
      ["/sys/devices/system/cpu/cpufreq/policy7/walt/hispeed_load"]
    ],
    "latency": [
      ["/sys/devices/system/cpu/cpufreq/policy0/walt/down_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy0/walt/up_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy4/walt/down_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy4/walt/up_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy7/walt/down_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy7/walt/up_rate_limit_us"]
    ],
    "target_loads": [
      ["/sys/devices/system/cpu/cpu0/cpufreq/walt/target_loads"],
      ["/sys/devices/system/cpu/cpu4/cpufreq/walt/target_loads"],
      ["/sys/devices/system/cpu/cpu7/cpufreq/walt/target_loads"]
    ],
    "target_load_thresh": [
      ["/sys/devices/system/cpu/cpu0/cpufreq/walt/target_load_thresh"],
      ["/sys/devices/system/cpu/cpu4/cpufreq/walt/target_load_thresh"],
      ["/sys/devices/system/cpu/cpu7/cpufreq/walt/target_load_thresh"]
    ],
    "min_freq": [
      ["$cpu_min_0"], ["$cpu_min_4"], ["$cpu_min_7"]
    ],
    "max_freq": [
      ["$cpu_max_0"], ["$cpu_max_4"], ["$cpu_max_7"]
    ],
    "cpu7_on": [
      ["$core_ctl_7", "#0"],
      ["$max_cpus_cpu7", "#1"],
      ["$min_cpus_cpu7", "#1"]
    ],
    "cpu7_off": [
      ["$core_ctl_7", "#0"],
      ["$min_cpus_cpu7", "#0"],
      ["$max_cpus_cpu7", "#0"]
    ],
    "cpu7_auto": [
      ["$core_ctl_7", "#1"],
      ["$min_cpus_cpu7", "#0"],
      ["$max_cpus_cpu7", "#1"]
    ],
    "common_active": [
      ["@preset", "cpu7_on"],
      ["@values", "latency", "0", "1000", "0", "1000", "0", "1000"],
      ["@uclamp", "0.00~max", "0.00~max", "0.5~max"]
    ],
    "common_inactive": [
      ["@values", "min_freq", "307200", "633600", "787200"],
      ["@values", "latency", "0", "8000", "0", "8000", "0", "8000"],
      ["@uclamp", "0.00~max", "0.00~max", "0.00~max"],
      ["$sched_boost", "0"],
      ["@values", "hispeed_load", "90", "90", "90"]
    ],
    "common_app": [
      ["$bus_ddr_min", "547000"],
      ["@cpuset", "0-3", "0-3", "0-7", "0-7"],
      ["$gpu_max_khz", "#680000000"],
      ["$mod_percent", "100"]
    ],
    "common_gaming": [
      ["@preset", "common_active"],
      ["@limiters", "NONE"],
      ["@uclamp", "0.00~max", "0.00~max", "0.00~max"],
      ["@cpuset", "0-3", "0-3", "0-5", "0-7"],
      ["$gpu_max_khz", "#990000000"],
      ["$sched_boost", "0"],
      ["$mod_percent", "100"]
    ],

    "powersave_active": [
      ["@preset", "common_active"],
      ["@values", "target_loads",
        "#-5 1555200:-10",
        "#-5 1651200:-10",
        "#-5 1651200:-10"
      ],
      ["@values", "hispeed_freq", "1228800", "1113600", "1171200"],
      ["@values", "hispeed_load", "90", "90", "90"]
    ],
    "powersave_inactive": [
      ["@preset", "common_inactive"],
      ["@preset", "cpu7_off"],
      ["@values", "target_loads",
        "#-10 1555200:-18",
        "#-10 1651200:-18",
        "#-10 1651200:-18"
      ],
      ["@values", "hispeed_freq", "0", "0", "0"]
    ],

    "balance_active": [
      ["@preset", "common_active"],
      ["@values", "target_loads",
        "#0 1670400:-10",
        "#0 2112000:-10",
        "#0 2131200:-10"
      ],
      ["@values", "hispeed_freq", "1228800", "1555200", "1651200"],
      ["@values", "hispeed_load", "90", "90", "90"]
    ],
    "balance_inactive": [
      ["@preset", "common_inactive"],
      ["@values", "target_loads",
        "#-10 1555200:-15 1670400:-20",
        "#-10 1651200:-18",
        "#-10 1651200:-18"
      ],
      ["@preset", "cpu7_auto"],
      ["@values", "hispeed_freq", "691200", "633600", "787200"]
    ],

    "performance_active": [
      ["@preset", "common_active"],
      ["@values", "target_loads",
        "#0 1670400:-10",
        "#0 2112000:-10",
        "#0 2131200:-10"
      ],
      ["@values", "hispeed_freq", "1228800", "1555200", "1651200"]
    ],
    "performance_inactive": [
      ["@preset", "common_inactive"],
      ["@values", "target_loads",
        "#-10 1555200:-15 1670400:-20",
        "#-10 1651200:-15",
        "#-10 1651200:-15"
      ],
      ["@values", "hispeed_freq", "1075200", "1324800", "1286400"],
      ["@values", "hispeed_load", "82", "82", "82"]
    ],

    "fast_active": [
      ["@preset", "common_active"],
      ["@values", "target_loads",
        "#5 1670400:-10",
        "#5 1766400:0 2112000:-8",
        "#5 1766400:0 2131200:-8"
      ],
      ["@values", "hispeed_freq", "1228800", "1555200", "1651200"],
      ["@values", "hispeed_load", "82", "82", "82"]
    ],
    "fast_inactive": [
      ["@preset", "common_inactive"],
      ["@values", "target_loads",
        "#-8 1555200:-10 1670400:-18",
        "#-8 1651200:-10 2112000:-16",
        "#-8 1651200:-10 2131200:-16"
      ],
      ["@values", "hispeed_freq", "1171200", "1440000", "1497600"]
    ]
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@preset", "reset"],
        ["$bus_ddr", "2736000"]
      ],
      "app": [
        ["@preset", "common_app"],
        ["@values", "max_freq", "1804800", "2112000", "2246400"]
      ],
      "game": [
        ["$bus_ddr_min", "547000"],
        ["@values", "hispeed_freq", "1056000", "1324800", "1401600"],
        ["$l3_boost", "#0"]
      ]
    },
    "balance": {
      "call": [
        ["@preset", "reset"]
      ],
      "app": [
        ["@preset", "common_app"],
        ["$bus_ddr", "2736000"],
        ["@values", "max_freq", "1804800", "2572800", "2707200"]
      ],
      "game": [
        ["$bus_ddr_min", "547000"],
        ["@values", "hispeed_freq", "1228800", "1651200", "1766400"],
        ["$l3_boost", "#0"],
        ["$bus_ddr", "3196000"]
      ]
    },
    "performance": {
      "call": [
        ["@preset", "reset"]
      ],
      "app": [
        ["@preset", "common_app"],
        ["$bus_ddr", "2736000"],
        ["@values", "max_freq", "1804800", "2745600", "2822400"]
      ],
      "game": [
        ["$bus_ddr_min", "547000"],
        ["$l3_boost", "#0"],
        ["$bus_ddr", "3196000"]
      ]
    },
    "fast": {
      "call": [
        ["@preset", "reset"]
      ],
      "app": [
        ["@preset", "common_app"],
        ["$bus_ddr", "3196000"],
        ["@values", "max_freq", "2016000", "2745600", "3187200"]
      ],
      "game": [
        ["$perfmgr_enable", "#0"],
        ["$l3_boost", "#1804800"],
        ["$bus_ddr", "3196000"],
        ["$bus_ddr_min", "2736000"]
      ]
    },
    "pedestal": {
      "call": [
        ["@preset", "reset"]
      ],
      "app": [
        ["@preset", "common_app"],
        ["$bus_ddr", "3196000"],
        ["@values", "max_freq", "2016000", "2745600", "3187200"]
      ],
      "game": [
        ["$perfmgr_enable", "#0"],
        ["$l3_boost", "#1804800"],
        ["$bus_ddr", "3196000"],
        ["$bus_ddr_min", "2736000"]
      ]
    }
  },
  "apps": [
    { "friendly": "极速响应", "categories": ["Launcher"], "import": "_Launcher.json" },
    { "friendly": "扫码", "categories": ["Scanner"], "import": "_Scanner.json" },
    { "friendly": "相机", "categories": ["Camera"], "import": "_Camera.json" },
    { "friendly": "狗屎/毒瘤", "categories": ["MiniProgram"], "import": "_MiniProgram.json" },
    { "friendly": "待机", "packages": ["standby"], "import": "_Standby.json" },
    { "friendly": "All", "packages": ["*"], "import": "_Apps.json" }
  ],
  "games": [
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Games.json"
    }
  ]
}