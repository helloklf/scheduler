{
  "platform": "mt6983",
  "platform_name": "D9000",
  "alias": {
    "gpu_freq": "/sys/kernel/ged/hal/custom_upbound_gpu_freq",
    "gpu_min_opp": "/sys/kernel/ged/hal/custom_boost_gpu_freq",
    "boost_affinity": "/sys/module/mtk_fpsgo/parameters/boost_affinity",
    "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
    "min_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/min_cpus",
    "ddr_freq_min": "/sys/class/devfreq/mtk-dvfsrc-devfreq/min_freq",
    "ddr_freq_cust": "/sys/class/devfreq/mtk-dvfsrc-devfreq/userspace/set_freq",
    "ged_kpi": "/sys/module/sspm_v3/holders/ged/parameters/is_GED_KPI_enabled",
    "fpsgo": "/sys/kernel/fpsgo/common/fpsgo_enable",
    "cpufreq": "/proc/cpudvfs/cpufreq_debug",
    "ddr_force_opp": "/sys/devices/platform/1c00f000.dvfsrc/1c00f000.dvfsrc:dvfsrc-helper/dvfsrc_force_vcore_dvfs_opp",
    "limit_cfreq": "/sys/kernel/fpsgo/fbt/limit_cfreq",
    "limit_rfreq": "/sys/kernel/fpsgo/fbt/limit_rfreq",
    "limit_cfreq_m": "/sys/kernel/fpsgo/fbt/limit_cfreq_m",
    "limit_rfreq_m": "/sys/kernel/fpsgo/fbt/limit_rfreq_m",
    "perfmgr_enable": "/sys/module/mtk_fpsgo/parameters/perfmgr_enable",
    "ddr_max": "/sys/class/devfreq/mtk-dvfsrc-devfreq/max_freq",
    "dcs": "/sys/kernel/ged/hal/dcs_mode"
  },
  "features": {
    "limiter": {
      "active_interval": 30,
      "gaming_interval": 30,
      "inactive_interval": 150,
      "ddr_boost": true,
      "params": [
        { "id": "p1:cpu0", "step": -0.9, "max": 1400000, "min": 400000, "margin": 200 },
        { "id": "p1:cpu4", "step": -0.9, "max": 2150000, "turning": 1600000, "min": 700000, "margin": 400 },
        { "id": "p1:cpu7", "step": -0.9, "max": 2300000, "turning": 1600000, "min": 1300000, "margin": 400 },

        { "id": "p2:cpu0", "max": 1500000, "min": 800000, "margin": 250 },
        { "id": "p2:cpu4", "max": 2400000, "turning": 2100000, "min": 1900000, "margin": 300 },
        { "id": "p2:cpu7", "max": 2450000, "turning": 2300000, "min": 2100000, "margin": 300 },

        { "id": "p3:cpu0", "max": 1600000, "min": 1400000, "margin": 270 },
        { "id": "p3:cpu4", "max": 2650000, "turning": 2400000, "min": 2100000, "margin": 300 },
        { "id": "p3:cpu7", "max": 2650000, "turning": 2450000, "min": 2100000, "margin": 300 },

        { "id": "p4:cpu0", "max": 1600000, "min": 1500000, "margin": 300 },
        { "id": "p4:cpu4", "max": 2650000, "min": 2450000, "margin": 300 },
        { "id": "p4:cpu7", "max": 2800000, "min": 2650000, "margin": 300 },


        { "id": "g1:cpu0", "max": 1400000, "turning": 1000000, "min": 500000, "margin": 280, "step": -1 },
        { "id": "g1:cpu4", "max": 2100000, "turning": 1800000, "min": 1300000, "margin": 230, "step": -1 },
        { "id": "g1:cpu7", "max": 2100000, "turning": 1800000, "min": 1300000, "margin": 250, "step": -1 },

        { "id": "g2:cpu0", "max": 1500000, "turning": 1200000, "min": 800000,  "margin": 300, "step": -1 },
        { "id": "g2:cpu4", "max": 2300000, "turning": 2300000, "min": 1700000, "margin": 300, "step": -1 },
        { "id": "g2:cpu7", "max": 2450000, "turning": 2100000, "min": 1700000, "margin": 300, "step": -1 },

        { "id": "g3:cpu0", "max": 1800000, "turning": 1600000, "min": 1000000,  "margin": 300, "step": -1, "smoothness": 4 },
        { "id": "g3:cpu4", "max": 2650000, "turning": 2450000, "min": 1900000, "margin": 350, "step": -1, "smoothness": 4 },
        { "id": "g3:cpu7", "max": 2650000, "turning": 2400000, "min": 1900000, "margin": 350, "step": -1, "smoothness": 4 },

        { "id": "g4:cpu0", "max": 1800000, "turning": 1600000, "min": 1200000, "margin": 280, "step": -1, "smoothness": 6 },
        { "id": "g4:cpu4", "max": 2850000, "turning": 2650000, "min": 2100000, "margin": 400, "step": -1, "smoothness": 6 },
        { "id": "g4:cpu7", "max": 2800000, "turning": 2650000, "min": 2100000, "margin": 400, "step": -1, "smoothness": 6 },


        { "id": "inactive:cpu0", "max": 1500000, "min": 400000, "margin": 100 },
        { "id": "inactive:cpu4", "max": 2100000, "min": 400000, "margin": 100 },
        { "id": "inactive:cpu7", "max": 2100000, "min": 1300000, "margin": 130 },

        { "id": "idle:cpu0", "max": 1200000, "min": 400000, "margin": 50 },
        { "id": "idle:cpu4", "max": 1800000, "min": 400000, "margin": 60 },
        { "id": "idle:cpu7", "max": 1800000, "min": 1300000, "margin": 70, "core_ctl": true }
      ]
    },
    "gesture_boost": {
      "enable": true,
      "enter": [
        ["/sys/kernel/ged/hal/custom_boost_gpu_freq", "28"],
        ["$ddr_freq_cust", "4266000000"]
      ],
      "exit": [
        ["/sys/kernel/ged/hal/custom_boost_gpu_freq", "99"],
        ["$ddr_freq_cust", "#0"]
      ]
    }
  },
  "presets": {
    "reset": [
      ["@mtk_reset"],
      ["@governor", "schedutil"],
      ["$boost_affinity", "0"],
      ["$ddr_force_opp", "999"],
      ["$ddr_freq_cust", "0"],
      ["$ddr_max", "#5500000000"]
    ],
    "min_freq": [
      ["$cpu_min_0"], ["$cpu_min_4"], ["$cpu_min_7"]
    ],
    "max_freq": [
      ["$cpu_max_0"], ["$cpu_max_4"], ["$cpu_max_7"]
    ],
    "cpu7_on": [
      ["$max_cpus_cpu7", "1"],
      ["$min_cpus_cpu7", "1"]
    ],
    "cpu7_off": [
      ["$min_cpus_cpu7", "0"],
      ["$max_cpus_cpu7", "0"]
    ],
    "common_actvie": [
      ["@preset", "cpu7_on"]
    ],
    "common_inactvie": [
      ["$gpu_min_opp", "99"]
    ],
    "comm_app": [
      ["/sys/kernel/cm_mgr/dbg_cm_mgr", "cm_mgr_enable 0|cm_mgr_perf_enable 0|dsu_mode_change 0"],
      ["/sys/kernel/ged/hal/custom_upbound_gpu_freq", "#20"],
      ["@cpuset", "0-3", "0-7", "0-7", "0-7"],
      ["/sys/kernel/ged/hal/timer_base_dvfs_margin", "#25"],
      ["/sys/kernel/ged/hal/loading_base_dvfs_step", "#4"],
      ["/sys/kernel/fpsgo/fstb/fstb_self_ctrl_fps_enable", "0"],
      ["/sys/kernel/fpsgo/fstb/set_render_no_ctrl", "1"],
      ["/sys/kernel/fpsgo/fstb/fstb_soft_level", "1 240-240"],
      ["/sys/kernel/fpsgo/fstb/set_render_max_fps", "144"]
    ],
    "comm_gaming": [
      ["/sys/kernel/cm_mgr/dbg_cm_mgr", "cm_mgr_enable 1|cm_mgr_perf_enable 1|dsu_mode_change 0"],
      ["@limiters", "NONE"],
      ["/sys/kernel/ged/hal/custom_upbound_gpu_freq", "#0"],
      ["/sys/kernel/ged/hal/custom_boost_gpu_freq", "99"],
      ["/sys/module/ged/parameters/gpu_cust_boost_freq",  "0"],
      ["@cpuset", "0-3", "0-5", "0-5", "0-7"],
      ["/sys/kernel/ged/hal/timer_base_dvfs_margin", "#15"],
      ["/sys/kernel/ged/hal/loading_base_dvfs_step", "#2"],
      ["$gpu_freq", "0"]
    ],

    "powersave_active": [
      ["@limiters", "p1:cpu0", "p1:cpu4", "p1:cpu7"],
      ["@preset", "common_actvie"],
      ["$ddr_freq_min", "#186600000"],
      ["@values", "min_freq", "400000", "700000", "1300000"]
    ],
    "powersave_inactive": [
      ["@limiters", "idle:cpu0", "idle:cpu4", "idle:cpu7"],
      ["@values", "min_freq", "200000", "400000", "1300000"],
      ["@preset", "common_inactvie"],
      ["$ddr_freq_min", "#800000000"],
      ["@preset", "cpu7_off"]
    ],
    "balance_active": [
      ["@limiters", "p2:cpu0", "p2:cpu4", "p2:cpu7"],
      ["@preset", "common_actvie"],
      ["$ddr_freq_min", "#266700000"],
      ["@values", "min_freq", "500000", "700000", "1300000"]
    ],
    "balance_inactive": [
      ["@limiters", "inactive:cpu0", "inactive:cpu4", "inactive:cpu7"],
      ["@values", "min_freq", "200000", "400000", "1300000"],
      ["@preset", "common_inactvie"],
      ["$ddr_freq_min", "#800000000"]
    ],
    "performance_active": [
      ["@limiters", "p3:cpu0", "p3:cpu4", "p3:cpu7"],
      ["@values", "min_freq", "700000", "900000", "1300000"],
      ["@preset", "common_actvie"],
      ["$ddr_freq_min", "#266700000"]
    ],
    "performance_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "common_inactvie"],
      ["$ddr_freq_min", "#800000000"],
      ["@cpu_freq", "cpu0", "200000", "1400000"],
      ["@cpu_freq", "cpu4", "400000", "2300000"],
      ["@cpu_freq", "cpu7", "1300000", "2300000"]
    ],
    "fast_active": [
      ["@limiters", "p4:cpu0", "p4:cpu4", "p4:cpu7"],
      ["@preset", "common_actvie"],
      ["$ddr_freq_min", "#320000000"],
      ["@values", "min_freq", "1000000", "1000000", "1300000"]
    ],
    "fast_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "common_inactvie"],
      ["$ddr_freq_min", "#1866000000"],
      ["@cpu_freq", "cpu0", "500000", "1800000"],
      ["@cpu_freq", "cpu4", "700000", "2650000"],
      ["@cpu_freq", "cpu7", "1300000", "2650000"]
    ]
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "powersave_active"]
      ],
      "app": [
        ["@preset", "comm_app"],
        ["$gpu_freq", "#20"]
      ],
      "game": [
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "400000", "1200000"],
        ["@cpu_freq", "cpu4", "400000", "1900000"],
        ["@cpu_freq", "cpu7", "1300000", "1900000"],
        ["$ddr_freq_min", "#1866000000"]
      ]
    },
    "balance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "balance_active"]
      ],
      "app": [
        ["@preset", "comm_app"],
        ["$gpu_freq", "#15"]
      ],
      "game": [
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "700000", "1500000"],
        ["@cpu_freq", "cpu4", "700000", "2400000"],
        ["@cpu_freq", "cpu7", "1300000", "2400000"],
        ["$ddr_freq_min", "#1866000000"]
      ]
    },
    "performance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "performance_active"]
      ],
      "app": [
        ["@preset", "comm_app"],
        ["$gpu_freq", "#10"]
      ],
      "game": [
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "800000", "1600000"],
        ["@cpu_freq", "cpu4", "1000000", "2650000"],
        ["@cpu_freq", "cpu7", "1300000", "2800000"]
      ]
    },
    "fast": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "fast_active"]
      ],
      "app": [
        ["@preset", "comm_app"],
        ["$gpu_freq", "#10"]
      ],
      "game": [
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "1000000", "1600000"],
        ["@cpu_freq", "cpu4", "1000000", "2850000"],
        ["@cpu_freq", "cpu7", "1300000", "3000000"],
        ["$ddr_freq_min", "#2667000000"]
      ]
    }
  },
  "apps": [
    {
      "friendly": "极速响应",
      "categories": ["Launcher"],
      "import": "_Launcher.json"
    },
    {
      "friendly": "待机",
      "packages": ["standby"],
      "import": "_Standby.json"
    },
    {
      "friendly": "扫码",
      "categories": ["Scanner"],
      "import": "_Scanner.json"
    },
    {
      "friendly": "相机",
      "categories": ["Camera"],
      "import": "_Camera.json"
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Apps.json"
    }
  ],
  "games": [
    {
      "friendly": "原神",
      "categories": ["GenshinImpact"],
      "import": "_GenshinImpact.json"
    },
    {
      "friendly": "星穹铁道",
      "packages": ["com.miHoYo.hkrpg", "com.miHoYo.hkrpg.bilibili", "com.HoYoverse.hkrpgoversea"],
      "import": "_HKRPG.json"
    },
    {
      "friendly": "和平精英",
      "packages": ["com.tencent.tmgp.pubgmhd", "com.tencent.tmgp.pubgmhdce",  "com.tencent.ig"],
      "import": "_PUBG.json"
    },
    {
      "friendly": "王者荣耀/LOL",
      "packages": ["com.tencent.tmgp.sgame", "com.garena.game.kgtw", "com.tencent.lolm"],
      "import": "_SGAME.json"
    },
    {
      "friendly": "QQ飞车",
      "packages": ["com.tencent.tmgp.speedmobile"],
      "import": "_SpeedMobile.json"
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_FAS.json"
    }
  ]
}