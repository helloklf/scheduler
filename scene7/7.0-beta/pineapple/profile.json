{
    "platform": "pineapple",
    "platform_name": "8GEN3",
    "framework": 220,
    "alias": {
      "gpu_max_khz": "/sys/class/kgsl/kgsl-3d0/devfreq/max_freq",
      "mod_percent": "/sys/class/kgsl/kgsl-3d0/devfreq/mod_percent",
      "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
      "min_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/min_cpus",
      "max_cpus_cpu5": "/sys/devices/system/cpu/cpu5/core_ctl/max_cpus",
      "min_cpus_cpu5": "/sys/devices/system/cpu/cpu5/core_ctl/min_cpus",
      "core_ctl_5": "/sys/devices/system/cpu/cpu5/core_ctl/enable",
      "perfmgr_enable": "/sys/module/perfmgr/parameters/perfmgr_enable",
      "cpu_min_2": "/sys/devices/system/cpu/cpufreq/policy2/scaling_min_freq",
      "cpu_max_2": "/sys/devices/system/cpu/cpufreq/policy2/scaling_max_freq",
      "cpu_min_5": "/sys/devices/system/cpu/cpufreq/policy5/scaling_min_freq",
      "cpu_max_5": "/sys/devices/system/cpu/cpufreq/policy5/scaling_max_freq",
      "sched_boost": "/proc/sys/walt/sched_boost",
      "glk_disable": "/sys/module/migt/parameters/glk_disable"
    },
    "features": {
      "limiter": {
        "active_interval": 30,
        "gaming_interval": 30,
        "inactive_interval": 100,
        "logger": false,
        "params": [
          { "id": "p1:cpu0", "step": -0.9, "max": 1689600, "min": 672000, "margin": 100 },
          { "id": "p1:cpu2", "step": -0.9, "max": 1920000, "min": 729600, "margin": 150 },
          { "id": "p1:cpu5", "step": -0.9, "max": 1920000, "min": 844800, "margin": 200 },
          { "id": "p1:cpu7", "step": -0.8, "max": 1824000, "min": 787200, "margin": 220 },
  
          { "id": "p2:cpu0", "step": -0.9, "max": 1804800, "min": 672000, "margin": 120 },
          { "id": "p2:cpu2", "step": -0.9, "max": 2438400, "min": 1785600, "margin": 200 },
          { "id": "p2:cpu5", "step": -0.7, "max": 2438400, "min": 1612800, "margin": 200 },
          { "id": "p2:cpu7", "step": -0.7, "max": 2438400, "min": 1708800, "margin": 200 },
  
          { "id": "p3:cpu0", "step": -0.9, "max": 1804800, "min": 1017600, "margin": 120 },
          { "id": "p3:cpu2", "step": -0.7, "max": 2707200, "min": 2131200, "margin": 300 },
          { "id": "p3:cpu5", "max": 2707200, "min": 1824000, "margin": 300 },
          { "id": "p3:cpu7", "max": 2726400, "min": 2112000, "margin": 300 },
  
          { "id": "p4:cpu0", "step": -0.9, "max": 1804800, "min": 902400, "margin": 150 },
          { "id": "p4:cpu2", "step": -0.7, "max": 2841600, "min": 2438400, "margin": 200 },
          { "id": "p4:cpu5", "max": 2841600, "min": 2438400, "margin": 200 },
          { "id": "p4:cpu7", "max": 2995200, "min": 2745600, "margin": 300 },
  
          { "id": "inactive:cpu0", "max": 1689600, "min": 672000, "margin": 100 },
          { "id": "inactive:cpu2", "max": 1785600, "min": 614400, "margin": 120 },
          { "id": "inactive:cpu5", "max": 1785600, "min": 614400, "margin": 120 },
          { "id": "inactive:cpu7", "max": 1708800, "min": 672000, "margin": 120 },
  
          { "id": "idle:cpu0", "max": 1689600, "min": 672000, "margin": 100 },
          { "id": "idle:cpu2", "max": 1785600, "min": 614400, "margin": 100 },
          { "id": "idle:cpu5", "max": 1785600, "min": 614400, "margin": 100, "core_ctl": true },
          { "id": "idle:cpu7", "max": 1708800, "min": 672000, "margin": 100, "core_ctl": true }
        ]
      },
      "gesture_boost": {
        "enable": true,
        "enter": [
          ["$sched_boost", "2"]
        ],
        "exit": [
          ["$sched_boost", "0"]
        ]
      }
    },
    "presets": {
      "reset": [
        ["@msm_reset"],
        ["@governor", "uag", "walt"],
        ["@preset", "cpu5_on"]
      ],
  
      "hispeed_freq": [
        ["/sys/devices/system/cpu/cpufreq/policy0/walt/hispeed_freq"],
        ["/sys/devices/system/cpu/cpufreq/policy2/walt/hispeed_freq"],
        ["/sys/devices/system/cpu/cpufreq/policy5/walt/hispeed_freq"],
        ["/sys/devices/system/cpu/cpufreq/policy7/walt/hispeed_freq"]
      ],
      "hispeed_load": [
        ["/sys/devices/system/cpu/cpufreq/policy0/walt/hispeed_load"],
        ["/sys/devices/system/cpu/cpufreq/policy2/walt/hispeed_load"],
        ["/sys/devices/system/cpu/cpufreq/policy5/walt/hispeed_load"],
        ["/sys/devices/system/cpu/cpufreq/policy7/walt/hispeed_load"]
      ],
      "min_freq": [
        ["$cpu_min_0"], ["$cpu_min_2"], ["$cpu_min_5"], ["$cpu_min_7"]
      ],
      "max_freq": [
        ["$cpu_max_0"], ["$cpu_max_2"], ["$cpu_max_5"], ["$cpu_max_7"]
      ],
      "cpu5_on": [
        ["$core_ctl_5", "1"],
        ["$max_cpus_cpu5", "#2"],
        ["$min_cpus_cpu5", "#2"]
      ],
      "cpu5_auto": [
        ["$core_ctl_5", "1"],
        ["$max_cpus_cpu5", "#2"],
        ["$min_cpus_cpu5", "#0"]
      ],
      "cpu7_on": [
        ["$core_ctl_7", "1"],
        ["$max_cpus_cpu7", "#1"],
        ["$min_cpus_cpu7", "#1"]
      ],
      "cpu7_off": [
        ["$core_ctl_7", "1"],
        ["$min_cpus_cpu7", "#0"],
        ["$max_cpus_cpu7", "#0"]
      ],
      "cpu7_auto": [
        ["$core_ctl_7", "1"],
        ["$min_cpus_cpu7", "#0"],
        ["$max_cpus_cpu7", "#1"]
      ],
      "common_active": [
        ["@preset", "cpu7_on"]
      ],
      "common_inactive": [
        ["@values", "min_freq", "307200", "480000", "480000", "480000"]
      ],
      "common_app": [
        ["$perfmgr_enable", "0"],
        ["@cpuset", "0-1", "0-6", "2-7", "0-7"],
        ["/dev/cpuset/sf/cpus", "2-6"]
      ],
      "common_gaming": [
        ["@limiters", "NONE"],
        ["@cpuset", "0-1", "0-3,5-6", "0-3,5-6", "2-7"],
        ["/dev/cpuset/sf/cpus", "2-3,5-6"],
        ["$sched_boost", "0"]
      ],

      "powersave_active": [
        ["@preset", "common_active"],
        ["@limiters", "p1:cpu0", "p1:cpu2", "p1:cpu5", "p1:cpu7"],
        ["@values", "hispeed_freq", "0", "844800", "844800", "844800"],
        ["@values", "hispeed_load", "85", "85", "85", "85"]
      ],
      "powersave_inactive": [
        ["@preset", "common_inactive"],
        ["@preset", "cpu7_off"],
        ["@limiters", "idle:cpu0", "idle:cpu2", "idle:cpu5", "idle:cpu7"],
        ["@values", "hispeed_freq", "0", "0", "0", "0"],
        ["@values", "hispeed_load", "90", "90", "90", "90"]
      ],

      "balance_active": [
        ["@preset", "common_active"],
        ["@limiters", "p2:cpu0", "p2:cpu2", "p2:cpu5", "p2:cpu7"],
        ["@values", "hispeed_freq", "0", "960000", "960000", "960000"],
        ["@values", "hispeed_load", "85", "85", "85", "85"]
      ],
      "balance_inactive": [
        ["@preset", "common_inactive"],
        ["@preset", "cpu7_auto"],
        ["@limiters", "inactive:cpu0", "inactive:cpu2", "inactive:cpu5", "inactive:cpu7"],
        ["@values", "hispeed_freq", "0", "729600", "729600", "787200"],
        ["@values", "hispeed_load", "90", "90", "90", "90"],
        ["@preset", "cpu7_off"],
        ["@preset", "cpu5_auto"]
      ],

      "performance_active": [
        ["@preset", "common_active"],
        ["@limiters", "p3:cpu0", "p3:cpu2", "p3:cpu5", "p3:cpu7"],
        ["@values", "hispeed_freq", "0", "1075200", "1075200", "1017600"],
        ["@values", "hispeed_load", "85", "85", "85", "85"]
      ],
      "performance_inactive": [
        ["@preset", "common_inactive"],
        ["@limiters", "p2:cpu0", "p2:cpu2", "p2:cpu5", "p2:cpu7"],
        ["@values", "hispeed_freq", "0", "844800", "844800", "844800"],
        ["@values", "hispeed_load", "90", "90", "90", "90"]
      ],

      "fast_active": [
        ["@preset", "common_active"],
        ["@limiters", "p4:cpu0", "p4:cpu2", "p4:cpu5", "p4:cpu7"],
        ["@values", "hispeed_freq", "0", "1075200", "1286400", "1248000"],
        ["@values", "hispeed_load", "82", "82", "82", "82"]
      ],
      "fast_inactive": [
        ["@preset", "common_inactive"],
        ["@limiters", "p3:cpu0", "p3:cpu2", "p3:cpu5", "p3:cpu7"],
        ["@values", "hispeed_freq", "0", "844800", "844800", "844800"],
        ["@values", "hispeed_load", "90", "90", "90", "90"]
      ],
      "freq_low": [
        ["@values", "max_freq", "1670400", "#1824000", "#1612800", "#1708800"],
        ["@values", "min_freq", "307200", "480000", "480000", "480000"]
      ],
      "freq_middle": [
        ["@values", "max_freq", "1670400", "#2131200", "#1824000", "#2092800"],
        ["@values", "min_freq", "441600", "614400", "614400", "614400"]
      ],
      "freq_high": [
        ["@values", "max_freq", "1804800", "#2438400", "#2246400", "#2476800"],
        ["@values", "min_freq", "672000", "729600", "614400", "787200"]
      ],
      "freq_extreme": [
        ["@values", "max_freq", "#1804800", "#2841600", "#2630400", "#2803200"],
        ["@values", "min_freq", "1056000", "960000", "844800", "902400"]
      ]
    },
    "schemes": {
      "powersave": {
        "call": [
          ["@preset", "reset"],
          ["@preset", "powersave_active"]
        ],
        "app": [
          ["@preset", "common_app"]
        ],
        "game": [
          ["@preset", "common_gaming"],
          ["@values", "hispeed_freq", "0", "0", "0", "0"],
          ["@preset", "freq_low"],
          ["$glk_disable", "0"]
        ]
      },
      "balance": {
        "call": [
          ["@preset", "reset"],
          ["@preset", "balance_active"]
        ],
        "app": [
          ["@preset", "common_app"]
        ],
        "game": [
          ["@preset", "common_gaming"],
          ["@values", "hispeed_freq", "0", "960000", "1113600", "1171200"],
          ["@preset", "freq_middle"],
          ["$glk_disable", "0"]
        ]
      },
      "performance": {
        "call": [
          ["@preset", "reset"],
          ["@preset", "performance_active"]
        ],
        "app": [
          ["@preset", "common_app"]
        ],
        "game": [
          ["@preset", "common_gaming"],
          ["@preset", "freq_high"],
          ["$glk_disable", "0"]
        ]
      },
      "fast": {
        "call": [
          ["@preset", "reset"],
          ["@preset", "fast_active"]
        ],
        "app": [
          ["@preset", "common_app"]
        ],
        "game": [
          ["$perfmgr_enable", "#0"],
          ["@preset", "common_gaming"],
          ["@preset", "freq_extreme"],
          ["$glk_disable", "1"]
        ]
      }
    },
    "apps": [
      { "friendly": "极速响应", "categories": ["Launcher"], "import": "_Launcher.json" },
      { "friendly": "扫码", "categories": ["Scanner"], "import": "_Scanner.json" },
      { "friendly": "相机", "categories": ["Camera"], "import": "_Camera.json" },
      { "friendly": "狗屎/毒瘤", "categories": ["MiniProgram"], "import": "_MiniProgram.json" },
      { "friendly": "待机", "packages": ["standby"], "import": "_Standby.json" },
      { "friendly": "All", "packages": ["*"], "import": "_Apps.json" }
    ],
    "games": [
      {
        "friendly": "王者荣耀/LOL M/QQ飞车/和平精英/星穹铁道/我的世界/使命召唤/暗区突围/穿越火线/王牌战士/光遇(CN)/光遇(Global)/蛋仔派对/决战平安京",
        "packages": [
          "com.tencent.tmgp.sgame", "com.garena.game.kgtw", "com.tencent.lolm",
          "com.tencent.tmgp.speedmobile",
          "com.tencent.tmgp.pubgmhd", "com.tencent.tmgp.pubgmhdce", "com.tencent.ig",
          "com.miHoYo.hkrpg", "com.miHoYo.hkrpg.bilibili", "com.HoYoverse.hkrpgoversea",
          "com.mojang.minecraftpe", 
          "com.tencent.tmgp.cod", "com.tencent.mf.uam", "com.tencent.tmgp.cf", "com.tencent.af",
          "com.netease.sky", "com.tgc.sky.android",
          "com.netease.party", "com.netease.moba.aligames"
        ],
        "import": "_FEAS.json"
      },
      {
        "friendly": "All",
        "packages": ["*"]
      }
    ]
  }