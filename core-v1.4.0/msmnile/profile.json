      "affinity": {
        "unity_main": "80",
        "comm": {
          "40": ["UnityGfxDevice"],
          "30": ["UnityMultiRende", "UnityPreload"],
          "3F": ["MIHOYO_NETWORK", "FMOD", "UnityChoreograp", "AsyncReadManage", "Worker Thread", "NativeThread"],
          "F": ["AudioTrack", "Audio", "tp_schedule"]
        },
        "other": "f"
      },
      "modes": [
        {
          "mode": ["performance", "fast", "pedestal"],
          "logger": true,
          "sensors": [
            {
              "sensor": "cpu",
              "interval": 100,
              "rules": [
                { "threshold": [-1, 96000], "enter": [["$gpu_max_khz", "450000000"]] },
                { "threshold": [95000, 89000], "enter": [["$gpu_max_khz", "530000000"]] },
                { "threshold": [88000, 83000], "enter": [["$gpu_max_khz", "630000000"]] },
                { "threshold": [78000, -1], "enter": [["$gpu_max_khz", "780000000"]] }
