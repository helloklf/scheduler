{
  "platform": "msmnile",
  "platform_name": "855/855+",
  "framework": 110,
  "alias": {
    "gpu_max_khz": "/sys/class/kgsl/kgsl-3d0/devfreq/max_freq",
    "cpu0_offline": "/sys/devices/system/cpu/cpu0/core_ctl/enable",
    "cpu4_offline": "/sys/devices/system/cpu/cpu4/core_ctl/enable",
    "cpu7_offline": "/sys/devices/system/cpu/cpu4/core_ctl/enable",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig"
  },
  "schemes": {
    "powersave": {
      "call": [
        ["$cpu4_offline", "1"],
        ["$cpu7_offline", "1"],
        ["@gpu_freq", "min", "510MHz"],
        ["@thermal_threshold", "90000"]
      ]
    },
    "balance": {
      "call": [
        ["$cpu4_offline", "0"],
        ["$cpu7_offline", "1"],
        ["@gpu_freq", "min", "610MHz"],
        ["@thermal_threshold", "95000"]
      ]
    },
    "performance": {
      "call": [
        ["$cpu4_offline", "0"],
        ["$cpu7_offline", "0"],
        ["@gpu_freq", "300MHz", "max"],
        ["@thermal_threshold", "97000"],
        ["$mi_thermal", "10"]
      ]
    },
    "fast": {
      "call": [
        ["$cpu4_offline", "0"],
        ["$cpu7_offline", "0"],
        ["@gpu_freq", "400MHz", "max"],
        ["@thermal_threshold", "100000"],
        ["$mi_thermal", "10"]
      ]
    },
    "pedestal": {
      "call": [
        ["$cpu4_offline", "0"],
        ["$cpu7_offline", "0"],
        ["@gpu_freq", "600MHz", "max"],
        ["@thermal_threshold", "105000"],
        ["$mi_thermal", "10"]
      ]
    }
  },
