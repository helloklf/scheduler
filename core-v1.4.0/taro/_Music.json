{
  "friendly": "音乐APP",
  "packages": [
    "com.netease.cloudmusic",
    "com.kugou.android",
    "com.kugou.android.lite"
  ],
  "sensors": [],
  "modes": [{
    "mode": ["powersave", "balance"],
    "booster": {
      "events": ["presets"],
      "duration": 1500,
      "enter": [
        ["$core_ctl_7", "0"],
        ["$cpuset_top", "0-7"],
        ["$cpuset_fg", "0-7"],
        ["@uclamp", "0.00~10.00", "0.00~max", "1.00~max"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["/sys/devices/system/cpu/cpu_boost/sched_boost_on_input", "0"],
        ["$cpuset_top", "0-6"],
        ["$cpuset_fg", "0-6"],
        ["$cpu_max_0", "1363200"],
        ["$cpu_max_4", "1555200"],
        ["$cpu_max_7", "1612800"],
        ["@uclamp", "0.00~0.00", "0.00~max", "0.00~max"],
        ["@high_rate", "off"]
      ]
    }
  },{
    "mode": ["performance", "fast"],
    "booster": {
      "events": ["presets"],
      "duration": 1500,
      "enter": [
        ["$cpuset_top", "0-7"],
        ["@uclamp", "0.00~20.00", "1.00~max", "1.00~max"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["@uclamp", "0.00~0.00", "0.00~max", "0.00~max"],
        ["/sys/devices/system/cpu/cpu_boost/sched_boost_on_input", "0"],
        ["$cpuset_top", "0-6"],
        ["$cpuset_fg", "0-6"],
        ["@high_rate", "off"]
      ]
    }
  }]
}