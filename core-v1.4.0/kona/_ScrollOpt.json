        ["$cpu_max_0", "1804800"],
        ["$cpu_max_4", "1958400"],
        ["$cpu_max_7", "2169600"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["$cpu_min_0", "300000"],
        ["@high_rate", "off"]
      ]
    }
  },{
    "mode": ["balance"],
    "booster": {
      "events": ["presets"],
      "duration": 2000,
      "idle_delay": 5000,
      "enter": [
        ["$stune_top_perf", "1"],
        ["$stune_top_boost", "10"],
