    "tv.danmaku.bili", "com.bilibili.app.in",
    "com.google.android.youtube",
    "com.tencent.qqlive",
    "com.ss.android.article.video",
    "com.qiyi.video",
    "com.qiyi.video.lite",
    "com.qiyi.video.sdkplayer",
    "com.dianshijia.newlive",
    "com.duowan.kiwi",
