{
  "platform": "taro",
  "platform_name": "8GEN1",
  "framework": 120,
  "schema": {
    "vendor": "msm",
    "cores": 8,
    "clusters": [0, 4, 7]
  },
  "alias": {
    "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig"
  },
  "features": {
    "high_rate": {
      "enable": true,
      "device_policy": [{
        "device": "cupid", "name": "Mi 12", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0",
        "1080P120Hz": "1", "1080P90Hz": "2", "1080P60Hz": "0"
      }, {
        "device": "ingres", "name": "K50G", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0",
        "1080P120Hz": "1", "1080P60Hz": "0"
      }]
    }
  },
  "schemes": {
    "powersave": {
      "call": [
        ["$core_ctl_4", "1"],
        ["$core_ctl_7", "1"],
        ["@thermal_disguise", "false"],
        ["$max_cpus_cpu7", "1"],
        ["@cpuset", "0", "0-3", "0-7", "0-7"],
        ["@uclamp", "0.00~1.00", "0.00~max", "0.00~max"],
        ["@gpu_freq", "min", "500MHz"]
      ]
    },
    "balance": {
      "call": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "1"],
        ["@thermal_disguise", "false"],
        ["$max_cpus_cpu7", "1"],
        ["@cpuset", "0-2", "0-3", "0-7", "0-7"],
        ["@uclamp", "0.00~max", "0.00~max", "0.00~max"],
        ["@gpu_freq", "min", "600MHz"]
      ]
    },
    "performance": {
      "call": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["@thermal_disguise", "false"],
        ["$max_cpus_cpu7", "1"],
        ["@cpuset", "0-1", "0-3", "0-7", "0-7"],
        ["@uclamp", "0.00~max", "0.10~max", "0.25~max"],
        ["@gpu_freq", "min", "750MHz"]
      ]
    },
    "fast": {
      "call": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["@thermal_disguise", "true"],
        ["$max_cpus_cpu7", "1"],
        ["$mi_thermal", "10"],
        ["@cpuset", "0", "0-3", "0-7", "0-7"],
        ["@uclamp", "0.00~max", "0.10~max", "0.5~max"],
        ["@gpu_freq", "300MHz", "max"]
      ]
    },
    "pedestal": {
      "call": [
        ["$core_ctl_4", "0"],
        ["$cpu7_offline", "0"],
        ["@thermal_disguise", "true"],
        ["$max_cpus_cpu7", "1"],
        ["$mi_thermal", "10"],
        ["@cpuset", "0", "0-3", "0-7", "0-7"],
        ["@uclamp", "0.00~max", "0.10~max", "2.0~max"],
