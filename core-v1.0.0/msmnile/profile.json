          "enter": {
            "$uclamp_fg_max": "max",
            "$uclamp_fg_min": "0.0",
            "$uclamp_bg_max": "max",
            "$uclamp_bg_min": "0.0",
            "$uclamp_top_max": "max",
            "$uclamp_top_min": "1.0",
            "$core_ctl_4": "0",
            "$core_ctl_7": "0"
          },
          "batter": 6,
          "infinite": {
            "$uclamp_top_min": "max",
            "$cpu_min_0": "1708800",
            "$cpu_min_4": "1497600",
            "$cpu_min_7": "1401600",
