{
    "friendly": "QQ飞车",
    "cpuset": {
      "interval": 5000,
      "unity_main": "7",
      "heavy_thread": "UnityGfx",
      "heavy_cores": "4-6",
      "comm": {
        "4-5": ["Job.Worker", "NativeThread", "mgp.speedmobile", "mali-", "GThread", "UnityChoreograp"]
      },
      "other": "0-7"
    },
    "limiters": [
      {"id": "speedmobile1:cpu4", "step_shift": -1, "mode": "UPPER",
        "max_freq": 2450000, "min_freq": 2300000, "margin_mhz": 190},

      {"id": "speedmobile2:cpu4", "step_shift": -1, "mode": "UPPER",
        "max_freq": 2650000, "min_freq": 2400000, "margin_mhz": 190},

      {"id": "speedmobile3:cpu4", "step_shift": -1, "mode": "UPPER",
        "max_freq": 2850000, "min_freq": 2450000, "margin_mhz": 190},

      {"id": "speedmobile4:cpu7", "step_shift": -1, "mode": "BOTTOM", "ddr_boost": true,
        "max_freq": 2650000, "min_freq": 1600000, "margin_mhz": 300, "smoothness": 6}
    ],
    "modes": [{
      "mode": ["powersave"],
      "call": [
        ["@values", "max_freq", "#2000000", "#2450000", "#2650000"],
        ["@limiters", "speedmobile1:cpu4"],
        ["@mtk_feas", "5"]
      ]
    }, {
      "mode": ["balance"],
      "call": [
        ["@values", "min_freq", "#1000000", "#1400000", "#50000"],
        ["@values", "max_freq", "#2000000", "#2450000", "#2650000"],
        ["@limiters", "speedmobile2:cpu4"],
        ["@mtk_feas", "2"]
      ]
    }, {
      "mode": ["performance"],
      "call": [
        ["@values", "min_freq", "#1000000", "#1400000", "#50000"],
        ["@values", "max_freq", "#2000000", "#2650000", "#2850000"],
        ["@limiters", "speedmobile3:cpu4"],
        ["@mtk_feas", "0"]
      ]
    }, {
      "mode": ["*"],
      "call": [
        ["@limiters", "speedmobile4:cpu7"]
      ]
    }]
}