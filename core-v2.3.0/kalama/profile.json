        { "id": "g1:cpu7", "step": -0.5, "max": 2092800, "min": 729600, "margin": 200, "mode": "performance" },

        { "id": "g2:cpu0", "max": 1555200, "min": 902400, "margin": 250 },
        { "id": "g2:cpu3", "max": 2457600, "min": 729600, "margin": 300, "mode": "performance" },
        { "id": "g2:cpu7", "max": 2476800, "min": 729600, "margin": 300, "mode": "performance" },

