{
  "platform": "mt6983",
  "platform_name": "D9000",
  "framework": 220,
  "alias": {
    "gpu_freq": "/sys/kernel/ged/hal/custom_upbound_gpu_freq",
    "boost_affinity": "/sys/module/mtk_fpsgo/parameters/boost_affinity",
    "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
    "min_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/min_cpus",
    "ddr_freq_min": "/sys/class/devfreq/mtk-dvfsrc-devfreq/min_freq",
    "ddr_freq_cust": "/sys/class/devfreq/mtk-dvfsrc-devfreq/userspace/set_freq",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig",
    "ged_kpi": "/sys/module/sspm_v3/holders/ged/parameters/is_GED_KPI_enabled",
    "fpsgo": "/sys/kernel/fpsgo/common/fpsgo_enable",
    "charge_limit": "/sys/class/power_supply/battery/charge_control_limit",
    "night_charging": "/sys/class/power_supply/battery/night_charging",
    "cpufreq": "/proc/cpudvfs/cpufreq_debug",
    "ddr_force_opp": "/sys/devices/platform/1c00f000.dvfsrc/1c00f000.dvfsrc:dvfsrc-helper/dvfsrc_force_vcore_dvfs_opp",
    "gpu_min_opp": "/sys/kernel/ged/hal/custom_boost_gpu_freq",
    "limit_cfreq": "/sys/kernel/fpsgo/fbt/limit_cfreq",
    "limit_rfreq": "/sys/kernel/fpsgo/fbt/limit_rfreq",
    "limit_cfreq_m": "/sys/kernel/fpsgo/fbt/limit_cfreq_m",
    "limit_rfreq_m": "/sys/kernel/fpsgo/fbt/limit_rfreq_m",
    "perfmgr_enable": "/sys/module/mtk_fpsgo/parameters/perfmgr_enable",
    "ddr_max": "/sys/class/devfreq/mtk-dvfsrc-devfreq/max_freq"
  },
  "features": {
    "high_rate": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_refresh_rate",
      "device_policy": []
    },
    "cpus_control": {
      "enable": true,
      "render_boost": true,
      "enable_control": "/data/local/tmp/scene_cpus_control"
    },
    "charge_control": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_charge_control",
      "suspend": [
        ["$charge_limit", "15"],
        ["$night_charging", "1"]
      ],
      "normal": [
        ["$charge_limit", "0"],
        ["$night_charging", "0"]
      ]
    },
    "limiter": {
      "active_interval": 20,
      "gaming_interval": 30,
      "inactive_interval": 150,
      "max_margin_ratio": 0.45,
      "ddr_boost": true,
      "logger": false,
      "params": [
        { "id": "p1:cpu0", "max_freq": 1400000, "min_freq": 500000, "margin_mhz": 250 },
        { "id": "p1:cpu4", "max_freq": 2150000, "min_freq": 700000, "margin_mhz": 300 },
        { "id": "p1:cpu7", "max_freq": 2300000, "min_freq": 1300000, "margin_mhz": 400 },

        { "id": "p2:cpu0", "max_freq": 1500000, "min_freq": 1050000, "margin_mhz": 300 },
        { "id": "p2:cpu4", "max_freq": 2400000, "min_freq": 2050000, "margin_mhz": 400 },
        { "id": "p2:cpu7", "max_freq": 2650000, "min_freq": 2100000, "margin_mhz": 400 },

        { "id": "p3:cpu0", "max_freq": 1600000, "min_freq": 1400000, "margin_mhz": 300 },
        { "id": "p3:cpu4", "max_freq": 2650000, "min_freq": 2400000, "margin_mhz": 300 },
        { "id": "p3:cpu7", "max_freq": 2800000, "min_freq": 2400000, "margin_mhz": 300 },

        { "id": "p4:cpu0", "max_freq": 1600000, "min_freq": 1500000, "margin_mhz": 300 },
        { "id": "p4:cpu4", "max_freq": 2650000, "min_freq": 2650000, "margin_mhz": 300 },
        { "id": "p4:cpu7", "max_freq": 2800000, "min_freq": 2650000, "margin_mhz": 300 },


        { "id": "g1:cpu0", "max_freq": 1400000, "min_freq": 500000, "margin_mhz": 250, "mode": "upper", "step_shift": -1 },
        { "id": "g1:cpu4", "max_freq": 2100000, "turning_freq": 1800000, "min_freq": 700000,  "margin_mhz": 230, "mode": "performance", "step_shift": -1 },
        { "id": "g1:cpu7", "max_freq": 2100000, "turning_freq": 1800000, "min_freq": 1300000, "margin_mhz": 250, "mode": "performance", "step_shift": -1 },

        { "id": "g2:cpu0", "max_freq": 1500000, "min_freq": 800000,  "margin_mhz": 250, "mode": "upper", "step_shift": -1 },
        { "id": "g2:cpu4", "max_freq": 2300000, "turning_freq": 2300000, "min_freq": 1000000, "margin_mhz": 265, "mode": "performance", "step_shift": -1 },
        { "id": "g2:cpu7", "max_freq": 2450000, "turning_freq": 2100000, "min_freq": 1300000, "margin_mhz": 265, "mode": "performance", "step_shift": -1 },

        { "id": "g3:cpu0", "max_freq": 1800000, "turning_freq": 1600000, "min_freq": 800000,  "margin_mhz": 250, "mode": "performance", "step_shift": -1 },
        { "id": "g3:cpu4", "max_freq": 2650000, "turning_freq": 2450000, "min_freq": 1400000, "margin_mhz": 300, "mode": "performance", "step_shift": -1 },
        { "id": "g3:cpu7", "max_freq": 2650000, "turning_freq": 2400000, "min_freq": 1600000, "margin_mhz": 300, "mode": "performance", "step_shift": -1 },

        { "id": "g4:cpu0", "max_freq": 1800000, "turning_freq": 1600000, "min_freq": 1050000, "margin_mhz": 280, "mode": "upper", "step_shift": -1 },
        { "id": "g4:cpu4", "max_freq": 2850000, "turning_freq": 2650000, "min_freq": 1400000, "margin_mhz": 400, "mode": "performance", "step_shift": -1 },
        { "id": "g4:cpu7", "max_freq": 2800000, "turning_freq": 2650000, "min_freq": 1600000, "margin_mhz": 400, "mode": "performance", "step_shift": -1 },


        { "id": "inactive:cpu0", "step_shift": -1, "max_freq": 1500000, "min_freq": 400000, "margin_mhz": 100 },
        { "id": "inactive:cpu4", "step_shift": -1, "max_freq": 2100000, "min_freq": 400000, "margin_mhz": 100 },
        { "id": "inactive:cpu7", "step_shift": -1, "max_freq": 2100000, "min_freq": 1300000, "margin_mhz": 130 },

        { "id": "idle:cpu0", "step_shift": -1, "max_freq": 1200000, "min_freq": 400000, "margin_mhz": 50 },
        { "id": "idle:cpu4", "step_shift": -1, "max_freq": 1800000, "min_freq": 400000, "margin_mhz": 60 },
        { "id": "idle:cpu7", "step_shift": -1, "max_freq": 1800000, "min_freq": 1300000, "margin_mhz": 70 }
      ]
    },
    "gesture_boost": {
      "enable": true,
      "enter": [
        ["/sys/kernel/ged/hal/custom_boost_gpu_freq", "28"],
        ["$ddr_freq_cust", "4266000000"]
      ],
      "exit": [
        ["/sys/kernel/ged/hal/custom_boost_gpu_freq", "99"],
        ["$ddr_freq_cust", "#0"]
      ]
    }
  },
  "presets": {
    "reset": [
      ["@mtk_reset"],
      ["@governor", "schedutil"],
      ["/sys/kernel/cm_mgr/dbg_cm_mgr", "cm_mgr_enable 1|cm_mgr_perf_enable 1|dsu_mode_change 0"],
      ["$ddr_force_opp", "999"],
      ["$ddr_freq_cust", "0"],
      ["$perfmgr_enable", "#0"]
    ],
    "min_freq": [
      ["$cpu_min_0"], ["$cpu_min_4"], ["$cpu_min_7"]
    ],
    "max_freq": [
      ["$cpu_max_0"], ["$cpu_max_4"], ["$cpu_max_7"]
    ],
    "kpi_on": [
      ["$ged_kpi", "#1"],
      ["$fpsgo", "#1"]
    ],
    "kpi_off": [
      ["$fpsgo", "#0"],
      ["$ged_kpi", "#0"]
    ],
    "cpu7_on": [
      ["$max_cpus_cpu7", "1"],
      ["$min_cpus_cpu7", "1"]
    ],
    "cpu7_off": [
      ["$min_cpus_cpu7", "0"],
      ["$max_cpus_cpu7", "0"]
    ],
    "fbt_limit": [
      ["$limit_cfreq", "2100000"], ["$limit_rfreq", "2450000"],
      ["$limit_cfreq_m", "2200000"], ["$limit_rfreq_m", "2450000"]
    ],
    "fbt_limit_off": [
      ["$limit_cfreq", "#0"], ["$limit_rfreq", "#0"],
      ["$limit_cfreq_m", "#0"], ["$limit_rfreq_m", "#0"]
    ],
    "common_actvie": [
      ["@preset", "cpu7_on"],
      ["@high_rate", "on"],
      ["$ddr_max", "#5500000000"]
    ],
    "common_inactvie": [
      ["$gpu_min_opp", "99"]
    ],
    "comm_app": [
      ["@cpuset", "0-3", "0-7", "0-7", "0-7"],
      ["/sys/kernel/ged/hal/timer_base_dvfs_margin", "#30"],
      ["/sys/kernel/ged/hal/loading_base_dvfs_step", "#4"]
    ],
    "comm_gaming": [
      ["@limiters", "NONE"],
      ["@preset", "kpi_off"],
      ["@preset", "fbt_limit_off"],
      ["/sys/kernel/ged/hal/custom_upbound_gpu_freq", "#0"],
      ["/sys/kernel/ged/hal/custom_boost_gpu_freq", "99"],
      ["/sys/module/ged/parameters/gpu_cust_boost_freq",  "0"],
      ["@cpuset", "0-3", "0-5", "0-5", "0-7"],
      ["$boost_affinity", "0"],
      ["/sys/kernel/ged/hal/timer_base_dvfs_margin", "#15"],
      ["/sys/kernel/ged/hal/loading_base_dvfs_step", "#4"]
    ],

    "powersave_active": [
      ["@limiters", "p1:cpu0", "p1:cpu4", "p1:cpu7"],
      ["@preset", "common_actvie"],
      ["$ddr_freq_min", "#186600000"],
      ["@values", "min_freq", "400000", "700000", "1300000"]
    ],
    "powersave_inactive": [
      ["@limiters", "idle:cpu0", "idle:cpu4"],
      ["@values", "min_freq", "200000", "400000", "1300000"],
      ["@preset", "common_inactvie"],
      ["@high_rate", "auto"],
      ["$ddr_freq_min", "#800000000"],
      ["@preset", "cpu7_off"]
    ],
    "balance_active": [
      ["@limiters", "p2:cpu0", "p2:cpu4", "p2:cpu7"],
      ["@preset", "common_actvie"],
      ["$ddr_freq_min", "#266700000"],
      ["@values", "min_freq", "500000", "700000", "1300000"]
    ],
    "balance_inactive": [
      ["@limiters", "inactive:cpu0", "inactive:cpu4", "inactive:cpu7"],
      ["@values", "min_freq", "200000", "400000", "1300000"],
      ["@preset", "common_inactvie"],
      ["$ddr_freq_min", "#800000000"],
      ["@high_rate", "auto"]
    ],
    "performance_active": [
      ["@limiters", "p3:cpu0", "p3:cpu4", "p3:cpu7"],
      ["@values", "min_freq", "700000", "900000", "1300000"],
      ["@preset", "common_actvie"],
      ["$ddr_freq_min", "#320000000"]
    ],
    "performance_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "common_inactvie"],
      ["$ddr_freq_min", "#800000000"],
      ["@cpu_freq", "cpu0", "200000", "1400000"],
      ["@cpu_freq", "cpu4", "400000", "2300000"],
      ["@cpu_freq", "cpu7", "1300000", "2300000"],
      ["@high_rate", "auto"]
    ],
    "fast_active": [
      ["@limiters", "p4:cpu0", "p4:cpu4", "p4:cpu7"],
      ["@preset", "common_actvie"],
      ["$ddr_freq_min", "#320000000"],
      ["@values", "min_freq", "1000000", "1000000", "1300000"]
    ],
    "fast_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "common_inactvie"],
      ["$ddr_freq_min", "#1866000000"],
      ["@cpu_freq", "cpu0", "500000", "1800000"],
      ["@cpu_freq", "cpu4", "700000", "2650000"],
      ["@cpu_freq", "cpu7", "1300000", "2650000"]
    ]
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "powersave_active"],
        ["$gpu_freq", "20"]
      ],
      "app": [
        ["@preset", "comm_app"]
      ],
      "game": [
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "400000", "1200000"],
        ["@cpu_freq", "cpu4", "400000", "1900000"],
        ["@cpu_freq", "cpu7", "1300000", "1900000"],
        ["$ddr_freq_min", "#1866000000"]
      ]
    },
    "balance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "balance_active"],
        ["$gpu_freq", "15"]
      ],
      "app": [
        ["@preset", "comm_app"]
      ],
      "game": [
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "700000", "1500000"],
        ["@cpu_freq", "cpu4", "700000", "2400000"],
        ["@cpu_freq", "cpu7", "1300000", "2400000"],
        ["$ddr_freq_min", "#1866000000"]
      ]
    },
    "performance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "performance_active"],
        ["$gpu_freq", "10"]
      ],
      "app": [
        ["@preset", "comm_app"]
      ],
      "game": [
        ["$mi_thermal", "13"],
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "800000", "1600000"],
        ["@cpu_freq", "cpu4", "1000000", "2650000"],
        ["@cpu_freq", "cpu7", "1300000", "2800000"]
      ]
    },
    "fast": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "fast_active"],
        ["$gpu_freq", "0"]
      ],
      "app": [
        ["@preset", "comm_app"]
      ],
      "game": [
        ["$mi_thermal", "10"],
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "1000000", "1600000"],
        ["@cpu_freq", "cpu4", "1000000", "2850000"],
        ["@cpu_freq", "cpu7", "1300000", "3000000"],
        ["$ddr_freq_min", "#2667000000"]
      ]
    }
  },
  "apps": [
    {
      "friendly": "极速响应",
      "categories": ["Launcher"],
      "import": "_Launcher.json"
    },
    {
      "friendly": "待机",
      "packages": ["standby"],
      "import": "_Standby.json"
    },
    {
      "friendly": "扫码",
      "categories": ["Scanner"],
      "import": "_Scanner.json"
    },
    {
      "friendly": "相机",
      "categories": ["Camera"],
      "import": "_Camera.json"
    },
    {
      "friendly": "备份还原",
      "packages": ["com.miui.huanji", "com.miui.backup"],
      "call": [
        ["@high_rate", "off"]
      ]
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Apps.json"
    }
  ],
  "games": [
    {
      "friendly": "原神",
      "categories": ["GenshinImpact"],
      "import": "_GenshinImpact.json"
    },
    {
      "friendly": "和平精英",
      "packages": ["com.tencent.tmgp.pubgmhd",  "com.tencent.ig"],
      "import": "_PUBG.json"
    },
    {
      "friendly": "王者荣耀/LOL",
      "packages": ["com.tencent.tmgp.sgame", "com.garena.game.kgtw", "com.tencent.lolm"],
      "import": "_SGAME.json"
    },
    {
      "friendly": "QQ飞车",
      "packages": ["com.tencent.tmgp.speedmobile"],
      "import": "_SpeedMobile.json"
    },
    {
      "remark": "Xiaomi Only (K50P/12PD)",
      "friendly": "我的世界/使命召唤/暗区突围/穿越火线/王牌战士/光遇(CN)/光遇(Global)/蛋仔派对/决战平安京",
      "packages": [
        "com.mojang.minecraftpe", 
        "com.tencent.tmgp.cod", "com.tencent.mf.uam", "com.tencent.tmgp.cf", "com.tencent.af",
        "com.netease.sky", "com.tgc.sky.android",
        "com.netease.party", "com.netease.moba.aligames"
      ],
      "import": "_FEAS.json"
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Games.json"
    }
  ]
}