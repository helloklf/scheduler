{
  "friendly": "一般APP",
  "packages": ["*"],
  "modes": [{
    "mode": ["powersave"],
    "booster": {
      "duration": 3000,
      "idle_delay": 1000,
      "enter": [
        ["@preset", "powersave_active"]
      ],
      "exit": [
        ["@preset", "powersave_inactive"]
      ]
    }
  }, {
    "mode": ["balance"],
    "booster": {
      "duration": 3000,
      "idle_delay": 2000,
      "enter": [
        ["@preset", "balance_active"]
      ],
      "exit": [
        ["@preset", "balance_inactive"]
      ]
    }
  }, {
    "mode": ["performance"],
    "booster": {
      "duration": 2000,
      "idle_delay": 2000,
      "enter": [
        ["@preset", "performance_active"]
      ],
      "exit": [
        ["@preset", "performance_inactive"]
      ]
    }
  }, {
    "mode": ["fast", "pedestal"],
    "booster": {
      "duration": 2000,
      "idle_delay": 2000,
      "enter": [
        ["$stune_top_boost", "100"],
        ["$sched_boost", "2"],
        ["@preset", "fast_active"]
      ],
      "exit": [
        ["@preset", "fast_inactive"]
      ]
    }
  }]
}