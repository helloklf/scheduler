{
  "platform": "mt6853",
  "platform_name": "MTK d720&800U",
  "framework": 210,
  "alias": {
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@cpu_freq", "policy0", "min", "-300MHz"],
        ["@cpu_freq", "policy6", "min", "-500MHz"],
        ["@gpu_freq", "min", "400MHz"],
        ["@set_priority", "top-app", "normal"],
        ["@set_priority", "foreground", "low"],
        ["@set_priority", "background", "min"]
      ]
    },
    "balance": {
      "call": [
        ["@cpu_freq", "policy0", "min", "-200MHz"],
        ["@cpu_freq", "policy6", "min", "-300MHz"],
        ["@gpu_freq", "min", "500MHz"],
        ["@set_priority", "top-app", "normal"],
        ["@set_priority", "foreground", "normal"],
        ["@set_priority", "background", "low"]
      ]
    },
    "performance": {
      "call": [
        ["@cpu_freq", "policy0", "min", "max"],
        ["@cpu_freq", "policy6", "min", "max"],
        ["@gpu_freq", "min", "700MHz"],
        ["@set_priority", "top-app", "high"],
        ["@set_priority", "foreground", "normal"],
        ["@set_priority", "background", "low"]
      ]
    },
    "fast": {
      "call": [
        ["@cpu_freq", "policy0", "max", "max"],
        ["@cpu_freq", "policy6", "max", "max"],
        ["@gpu_freq", "min", "900MHz"],
        ["@set_priority", "top-app", "max"],
        ["@set_priority", "foreground", "high"],
        ["@set_priority", "background", "normal"]
      ]
    },
    "pedestal": {
      "call": [
        ["@cpu_freq", "policy0", "max", "max"],
        ["@cpu_freq", "policy6", "max", "max"],
        ["@gpu_freq", "min", "900MHz"],
        ["@set_priority", "top-app", "max"],
        ["@set_priority", "foreground", "high"],
        ["@set_priority", "background", "normal"]
      ]
    }
  },
  "apps": [
    {
      "friendly": "原神",
      "categories": ["GenshinImpact"],
      "modes": [
        {
          "mode": ["powersave", "balance", "performance"],
          "cpuset": {
            "comm": {
              "6-7": ["UnityMain"],
              "6": ["UnityGfx"],
              "5": ["UnityMultiRende"],
              "2-4": ["Worker Thread"]
            },
            "other": "0-5"
          }
        },
        {
          "mode": ["fast", "pedestal"],
          "cpuset": {
            "comm": {
              "6-7": ["UnityMain"],
              "6": ["UnityGfx", "UnityMultiRende"]
            },
            "other": "0-7"
          }
        }
      ]
    },
    {
      "friendly": "王者荣耀",
      "packages": ["com.tencent.tmgp.sgame"],
      "cpuset": {
        "interval": 1000,
        "comm": {
          "6-7": ["UnityGfx", "UnityMain", "UnityPreload", "Thread-"],
          "3-5": ["Worker Thread", "NativeThread", "Audio", "NDK Media", "GVoice", "FMOD mixer", "FMOD stream", "ff_read"]
        },
        "other": "0-7"
      }
    },
    {
      "friendly": "和平精英",
      "packages": ["com.tencent.tmgp.pubgmhd",  "com.tencent.ig"],
      "cpuset": {
        "interval": 1000,
        "comm": {
          "7": ["RenderThread"],
          "4-6": ["Thread-", "TaskGraphNP"]
        },
        "other": "0-5"
      },
      "sensors": [],
      "modes": [
        {
          "mode": ["fast"],
          "logger": false,
          "booster": {
            "events": ["presets"],
            "duration": 2000,
            "enter": [
              ["$stune_top_boost", "60"]
            ],
            "exit": [
              ["$sched_boost", "0"]
            ]
          }
        }
      ]
    }
  ]
}