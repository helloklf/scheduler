{
  "platform": "taro",
  "platform_name": "8GEN1",
  "framework": 220,
  "alias": {
    "gpu_max_khz": "/sys/class/kgsl/kgsl-3d0/devfreq/max_freq",
    "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
    "min_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/min_cpus",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig",
    "charge_limit": "/sys/class/power_supply/battery/charge_control_limit",
    "night_charging": "/sys/class/power_supply/battery/night_charging",
    "bus_ddr_min": "/sys/devices/system/cpu/bus_dcvs/DDR/19091000.qcom,bwmon-ddr/min_freq",
    "bus_ddr": "/sys/devices/system/cpu/bus_dcvs/DDR/19091000.qcom,bwmon-ddr/max_freq",
    "l3_boost": "/sys/devices/system/cpu/bus_dcvs/L3/boost_freq",
    "mod_percent": "/sys/class/kgsl/kgsl-3d0/devfreq/mod_percent"
  },
  "features": {
    "high_rate": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_refresh_rate",
      "device_policy": [{
        "device": "cupid", "name": "Mi 12", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0", "mode_standby": "0",
        "1080P120Hz": "1", "1080P90Hz": "2", "1080P60Hz": "0"
      }, {
        "device": "ingres", "name": "K50G", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0", "mode_standby": "0",
        "1080P120Hz": "1", "1080P60Hz": "0"
      }]
    },
    "cpus_control": {
      "enable": true,
      "render_boost": true,
      "enable_control": "/data/local/tmp/scene_cpus_control"
    },
    "limiter": {
      "active_interval": 24,
      "gaming_interval": 30,
      "inactive_interval": 150,
      "logger": false,
      "params": [
        { "id": "p1:cpu0", "step_shift": -0.9, "max_freq": 1267200, "min_freq": 614400, "margin_mhz": 160 },
        { "id": "p1:cpu4", "max_freq": 1766400, "min_freq": 768000, "margin_mhz": 250 },
        { "id": "p1:cpu7", "max_freq": 1843200, "min_freq": 1286400, "margin_mhz": 300 },

        { "id": "p2:cpu0", "step_shift": -0.8, "max_freq": 1478400, "min_freq": 729600, "margin_mhz": 210 },
        { "id": "p2:cpu4", "max_freq": 2112000, "min_freq": 1766400, "margin_mhz": 330 },
        { "id": "p2:cpu7", "max_freq": 2169600, "min_freq": 1728000, "margin_mhz": 380 },

        { "id": "p3:cpu0", "max_freq": 1689600, "min_freq": 1075200, "margin_mhz": 280 },
        { "id": "p3:cpu4", "max_freq": 2112000, "min_freq": 1996800, "margin_mhz": 380 },
        { "id": "p3:cpu7", "max_freq": 2400000, "min_freq": 2054400, "margin_mhz": 420 },

        { "id": "p4:cpu0", "max_freq": 1785600, "min_freq": 1574400, "margin_mhz": 100 },
        { "id": "p4:cpu4", "max_freq": 2496000, "min_freq": 2112000, "margin_mhz": 400 },
        { "id": "p4:cpu7", "max_freq": 2995200, "min_freq": 2169600, "margin_mhz": 450 },


        { "id": "g1:cpu0", "step_shift": -1, "max_freq": 1574400, "min_freq": 729600, "margin_mhz": 245, "smoothness": 8, "mode": "performance" },
        { "id": "g1:cpu4", "step_shift": -1, "max_freq": 1651200, "min_freq": 883200, "margin_mhz": 245, "smoothness": 4, "mode": "performance" },
        { "id": "g1:cpu7", "step_shift": -1, "max_freq": 1728000, "min_freq": 806400, "margin_mhz": 185, "smoothness": 4, "mode": "performance" },

        { "id": "g2:cpu0", "step_shift": -1, "max_freq": 1574400, "min_freq": 960000, "margin_mhz": 265, "smoothness": 6, "mode": "performance" },
        { "id": "g2:cpu4", "step_shift": -1, "max_freq": 1881600, "min_freq": 883200, "margin_mhz": 285, "smoothness": 4, "mode": "performance" },
        { "id": "g2:cpu7", "step_shift": -1, "max_freq": 1958400, "min_freq": 940800, "margin_mhz": 225, "smoothness": 4, "mode": "performance" },

        { "id": "g3:cpu0", "step_shift": -1, "max_freq": 1574400, "min_freq": 960000, "margin_mhz": 280, "smoothness": 8, "mode": "performance" },
        { "id": "g3:cpu4", "step_shift": -1, "max_freq": 1996800, "min_freq": 883200, "margin_mhz": 300, "smoothness": 5, "mode": "performance" },
        { "id": "g3:cpu7", "step_shift": -1, "max_freq": 2169600, "min_freq": 940800, "margin_mhz": 260, "smoothness": 5, "mode": "performance" },

        { "id": "g4:cpu0", "step_shift": -1, "max_freq": 1689600, "min_freq": 960000, "margin_mhz": 280, "smoothness": 8, "mode": "performance" },
        { "id": "g4:cpu4", "step_shift": -1, "max_freq": 2112000, "min_freq": 883200, "margin_mhz": 350, "smoothness": 5, "mode": "performance" },
        { "id": "g4:cpu7", "step_shift": -1, "max_freq": 2400000, "min_freq": 940800, "margin_mhz": 300, "smoothness": 5, "mode": "performance" },


        { "id": "inactive:cpu0", "max_freq": 1574400, "margin_mhz": 100 },
        { "id": "inactive:cpu4", "max_freq": 1766400, "margin_mhz": 100 },
        { "id": "inactive:cpu7", "max_freq": 1958400, "margin_mhz": 120 },

        { "id": "idle:cpu0", "max_freq": 1171200, "margin_mhz": 40 },
        { "id": "idle:cpu4", "max_freq": 1651200, "margin_mhz": 60 },
        { "id": "idle:cpu7", "max_freq": 1728000, "margin_mhz": 80 }
      ]
    }
  },
  "presets": {
    "reset": [
      ["@msm_reset"],
      ["$bus_ddr_min", "547000"],
      ["$l3_boost", "0"],
      ["@governor", "walt"],
      ["@values", "latency", "0", "1000", "0", "1000", "0", "1000"],
      ["$mod_percent", "100"],
      ["@values", "target_loads", "0", "0", "0"]
    ],

    "hispeed_freq": [
      ["/sys/devices/system/cpu/cpufreq/policy0/walt/hispeed_freq"],
      ["/sys/devices/system/cpu/cpufreq/policy4/walt/hispeed_freq"],
      ["/sys/devices/system/cpu/cpufreq/policy7/walt/hispeed_freq"]
    ],
    "hispeed_load": [
      ["/sys/devices/system/cpu/cpufreq/policy0/walt/hispeed_load"],
      ["/sys/devices/system/cpu/cpufreq/policy4/walt/hispeed_load"],
      ["/sys/devices/system/cpu/cpufreq/policy7/walt/hispeed_load"]
    ],
    "latency": [
      ["/sys/devices/system/cpu/cpufreq/policy0/walt/down_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy0/walt/up_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy4/walt/down_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy4/walt/up_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy7/walt/down_rate_limit_us"],
      ["/sys/devices/system/cpu/cpufreq/policy7/walt/up_rate_limit_us"]
    ],
    "target_loads": [
      ["/sys/devices/system/cpu/cpu0/cpufreq/walt/target_loads"],
      ["/sys/devices/system/cpu/cpu4/cpufreq/walt/target_loads"],
      ["/sys/devices/system/cpu/cpu7/cpufreq/walt/target_loads"]
    ],
    "min_freq": [
      ["$cpu_min_0"], ["$cpu_min_4"], ["$cpu_min_7"]
    ],
    "max_freq": [
      ["$cpu_max_0"], ["$cpu_max_4"], ["$cpu_max_7"]
    ],
    "cpu7_on": [
      ["$core_ctl_7", "0"],
      ["$max_cpus_cpu7", "1"],
      ["$min_cpus_cpu7", "1"]
    ],
    "cpu7_off": [
      ["$core_ctl_7", "0"],
      ["$min_cpus_cpu7", "0"],
      ["$max_cpus_cpu7", "0"]
    ],
    "cpu7_auto": [
      ["$core_ctl_7", "1"],
      ["$min_cpus_cpu7", "0"],
      ["$max_cpus_cpu7", "1"]
    ],
    "common_active": [
      ["@preset", "cpu7_on"],
      ["@uclamp", "0.00~max", "0.10~max", "1.00~max"],
      ["@values", "latency", "0", "1000", "0", "1000", "0", "1000"],
      ["@cpuset", "0-3", "0-3", "1-7", "0-7"]
    ],
    "common_inactive": [
      ["@values", "latency", "0", "8000", "0", "8000", "0", "8000"],
      ["@uclamp", "0.00~max", "0.0~max", "0.0~max"],
      ["@cpuset", "0-3", "0-3", "1-6", "0-7"]
    ],
    "common_app": [],
    "common_gaming": [
      ["@uclamp", "0.00~max", "0.0~max", "0.0~max"],
      ["@cpuset", "0-3", "1-3", "0-5", "0-7"]
    ],

    "powersave_active": [
      ["@preset", "common_active"],
      ["@limiters", "p1:cpu0", "p1:cpu4", "p1:cpu7"],
      ["@preset", "perf_fg_opt"],
      ["@values", "hispeed_freq", "614400", "633600", "806400"],
      ["@values", "hispeed_load", "90", "90", "90"]
    ],
    "powersave_inactive": [
      ["@preset", "common_inactive"],
      ["@limiters", "idle:cpu0", "idle:cpu4"],
      ["@high_rate", "auto"],
      ["@values", "min_freq", "307200", "633600", "806400"],
      ["@preset", "cpu7_off"],
      ["@values", "hispeed_freq", "0", "0", "0"],
      ["@values", "hispeed_load", "90", "90", "90"]
    ],

    "balance_active": [
      ["@preset", "common_active"],
      ["@limiters", "p2:cpu0", "p2:cpu4", "p2:cpu7"],
      ["@values", "hispeed_freq", "614400", "1113600", "1171200"],
      ["@values", "hispeed_load", "85", "85", "85"]
    ],
    "balance_inactive": [
      ["@preset", "common_inactive"],
      ["@values", "min_freq", "307200", "633600", "806400"],
      ["@limiters", "inactive:cpu0", "idle:cpu4", "idle:cpu7"],
      ["@preset", "cpu7_auto"],
      ["@values", "hispeed_freq", "614400", "633600", "806400"],
      ["@values", "hispeed_load", "90", "90", "90"],
      ["@high_rate", "auto"]
    ],


    "performance_active": [
      ["@preset", "common_active"],
      ["@limiters", "p3:cpu0", "p3:cpu4", "p3:cpu7"],
      ["@values", "hispeed_freq", "1075200", "1324800", "1286400"],
      ["@values", "hispeed_load", "82", "82", "82"]
    ],
    "performance_inactive": [
      ["@preset", "common_inactive"],
      ["@values", "min_freq", "307200", "633600", "806400"],
      ["@limiters", "p2:cpu0", "p2:cpu4", "p2:cpu7"],
      ["@values", "hispeed_freq", "1075200", "1324800", "1286400"],
      ["@values", "hispeed_load", "82", "82", "82"],
      ["@high_rate", "auto"]
    ],

    "fast_active": [
      ["@preset", "common_active"],
      ["@limiters", "p4:cpu0", "p4:cpu4", "p4:cpu7"],
      ["@values", "hispeed_freq", "1075200", "1440000", "1497600"],
      ["@values", "hispeed_load", "82", "82", "82"]
    ],
    "fast_inactive": [
      ["@preset", "common_inactive"],
      ["@values", "min_freq", "614400", "633600", "806400"],
      ["@limiters", "p3:cpu0", "p3:cpu4", "p3:cpu7"],
      ["@values", "hispeed_freq", "1075200", "1324800", "1286400"],
      ["@values", "hispeed_load", "82", "82", "82"],
      ["@high_rate", "auto"]
    ]
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "powersave_active"],
        ["@gpu_freq", "min", "500MHz"]
      ],
      "app": [
        ["$bus_ddr", "1708000"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["$bus_ddr", "2092000"],
        ["$mod_percent", "92"],
        ["@values", "latency", "0", "3000", "0", "3000", "0", "3000"]
      ]
    },
    "balance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "balance_active"],
        ["@gpu_freq", "min", "600MHz"]
      ],
      "app": [
        ["$bus_ddr", "2092000"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["$bus_ddr", "2736000"],
        ["$mi_thermal", "13"],
        ["$mod_percent", "92"],
        ["@values", "latency", "0", "2000", "0", "2000", "0", "2000"]
      ]
    },
    "performance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "performance_active"],
        ["@gpu_freq", "min", "750MHz"]
      ],
      "app": [
        ["$bus_ddr", "2736000"]
      ],
      "game": [
        ["@preset", "common_gaming"],
        ["$mi_thermal", "13"],
        ["$bus_ddr", "2736000"],
        ["$mod_percent", "92"],
        ["@values", "latency", "0", "2000", "0", "2000", "0", "2000"]
      ]
    },
    "fast": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "fast_active"],
        ["$mi_thermal", "10"],
        ["@gpu_freq", "min", "max"],
        ["$bus_ddr", "3196000"]
      ],
      "app": [
        ["@charge", "normal"]
      ],
      "game": [
        ["$l3_boost", "1651200"],
        ["$bus_ddr_min", "2736000"],
        ["@preset", "common_gaming"],
        ["$mi_thermal", "10"],
        ["$mod_percent", "93"],
        ["@values", "latency", "0", "2000", "0", "2000", "0", "2000"]
      ]
    }
  },
  "apps": [
    {
      "fir": "模拟器",
      "packages": ["xyz.aethersx2.android", "com.aiwu.dolphinemu"],
      "import": "_Simulate.json"
    },
    {
      "friendly": "视频应用/社交/音乐/阅读/短视频",
      "categories": ["Video", "IM", "Music", "Reader", "ShortVideo"],
      "import": "_Apps.json"
    },
    {
      "friendly": "提高滑动性能",
      "categories": ["ScrollOpt"],
      "import": "_Apps.json"
    },
    {
      "friendly": "极速响应",
      "categories": ["Launcher"],
      "import": "_Launcher.json"
    },
    {
      "friendly": "扫码",
      "categories": ["Scanner"],
      "import": "_Scanner.json"
    },
    {
      "friendly": "相机",
      "categories": ["Camera"],
      "import": "_Camera.json"
    },
    {
      "friendly": "狗屎/毒瘤",
      "categories": ["MiniProgram"],
      "import": "_MiniProgram.json"
    },
    {
      "friendly": "待机",
      "packages": ["standby"],
      "import": "_Standby.json"
    },
    {
      "friendly": "备份还原",
      "packages": ["com.miui.huanji", "com.miui.backup"],
      "call": [
        ["@uclamp", "0.00~max", "0.00~max", "0.00~max"],
        ["@high_rate", "off", "2000"]
      ]
    },
    {
      "friendly": "白名单应用",
      "packages": ["com.android.providers.downloads.ui", "android.process.mediaUI"],
      "import": "_WhiteList.json"
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Apps.json"
    }
  ],
  "games": [
    {
      "friendly": "原神",
      "categories": ["GenshinImpact"],
      "import": "_GenshinImpact.json"
    },
    {
      "friendly": "和平精英",
      "packages": ["com.tencent.tmgp.pubgmhd",  "com.tencent.ig"],
      "cpuset": {
        "heavy_thread": "RenderThread",
        "heavy_cores": "7",
        "comm": {
          "4-6": ["Thread-", "TaskGraphNP"]
        },
        "other": "0-6"
      }
    },
    {
      "friendly": "王者荣耀/LOL",
      "packages": ["com.tencent.tmgp.sgame"],
      "import": "_SGAME.json"
    },
    {
      "friendly": "QQ飞车",
      "packages": ["com.tencent.tmgp.speedmobile"],
      "import": "_SpeedMobile.json"
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Games.json"
    }
  ]
}