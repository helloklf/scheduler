{
  "friendly": "QQ飞车",
  "cpuset": {
    "interval": 3000,
    "unity_main": "7",
    "heavy_thread": "UnityGfx",
    "heavy_cores": "6",
    "other": "0-5"
  },
  "call": [
    ["@preset", "kpi_off"]
  ],
  "limiters": [
    {"id": "speedmobile:cpu4", "step_shift": -1,
      "max_freq": 2100000, "min_freq": 1700000, "margin_mhz": 400, "smoothness": 6},
    {"id": "speedmobile:cpu7", "step_shift": -1, "ddr_boost": true,
      "max_freq": 2400000, "min_freq": 1900000, "margin_mhz": 300, "smoothness": 6},

    {"id": "speedmobile1:cpu4", "step_shift": -1,
      "max_freq": 2300000, "min_freq": 1900000, "margin_mhz": 400, "smoothness": 6},
    {"id": "speedmobile1:cpu7", "step_shift": -1, "ddr_boost": true,
      "max_freq": 2650000, "min_freq": 2100000, "margin_mhz": 300, "smoothness": 6},

    {"id": "speedmobile2:cpu4", "step_shift": -1,
      "max_freq": 2650000, "min_freq": 2100000, "margin_mhz": 400, "smoothness": 6},
    {"id": "speedmobile2:cpu7", "step_shift": -1, "ddr_boost": true,
      "max_freq": 2800000, "min_freq": 2300000, "margin_mhz": 300, "smoothness": 6},

    {"id": "speedmobile3:cpu7", "step_shift": -1, "mode": "BOTTOM", "ddr_boost": true,
      "boost_freq": 2650000, "max_freq": 1700000, "margin_mhz": 250, "smoothness": 6}
  ],
  "modes": [{
    "mode": ["powersave"],
    "call": [
      ["@values", "max_freq", "#1400000", "#2100000", "#2400000"],
      ["@limiters", "speedmobile:cpu4", "speedmobile:cpu7"],
      ["@mtk_feas", "15"]
    ]
  }, {
    "mode": ["balance"],
    "call": [
      ["@values", "max_freq", "#1500000", "#2300000", "#2650000"],
      ["@limiters", "speedmobile1:cpu4", "speedmobile1:cpu7"],
      ["@mtk_feas", "10"]
    ]
  }, {
    "mode": ["performance"],
    "call": [
      ["@values", "max_freq", "#1600000", "#2450000", "#2800000"],
      ["@limiters", "speedmobile1:cpu4", "speedmobile2:cpu7"],
      ["@mtk_feas", "2"]
    ]
  }, {
    "mode": ["fast"],
    "call": [
      ["@values", "max_freq", "#1600000", "#2650000", "#3000000"],
      ["@limiters", "speedmobile2:cpu4", "speedmobile3:cpu7"]
    ]
  }]
}