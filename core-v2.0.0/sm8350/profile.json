{
  "platform": "lahaina",
  "platform_name": "888/888+",
  "framework": 210,
  "alias": {
    "gpu_max_khz": "/sys/class/kgsl/kgsl-3d0/devfreq/max_freq",
    "max_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/max_cpus",
    "min_cpus_cpu7": "/sys/devices/system/cpu/cpu7/core_ctl/min_cpus",
    "max_cpus_cpu4": "/sys/devices/system/cpu/cpu4/core_ctl/max_cpus",
    "min_cpus_cpu4": "/sys/devices/system/cpu/cpu4/core_ctl/min_cpus",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig",
    "charge_limit": "/sys/class/power_supply/battery/charge_control_limit",
    "night_charging": "/sys/class/power_supply/battery/night_charging",
    "msm_min_freq": "/sys/module/msm_performance/parameters/cpu_min_freq",
    "msm_max_freq": "/sys/module/msm_performance/parameters/cpu_max_freq",
    "llcc_ddr_max": "/sys/class/devfreq/soc:qcom,cpu-llcc-ddr-bw/max_freq",
    "cpu_llcc_max": "/sys/class/devfreq/soc:qcom,cpu-cpu-llcc-bw/max_freq",
    "llcc_ddr_min": "/sys/class/devfreq/soc:qcom,cpu-llcc-ddr-bw/min_freq",
    "cpu_llcc_min": "/sys/class/devfreq/soc:qcom,cpu-cpu-llcc-bw/min_freq"
  },
  "features": {
    "high_rate": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_refresh_rate",
      "device_policy": [
        {
          "device": "vili", "name": "Mi 11 Pro/Ultra(Global)", "enable": true,
          "mode_high_rate": "1", "mode_low_rate": "0", "mode_standby": "0"
        },
        {
          "device": "star", "name": "Mi 11 Pro/Ultra", "enable": true,
          "mode_high_rate": "0", "mode_low_rate": "2", "mode_standby": "0",
          "1440P120Hz": "0", "1440P90Hz": "1", "1440P60Hz": "2"
        }, {
          "device": "mars", "name": "Mi 11 Pro/Ultra", "enable": true,
          "mode_high_rate": "0", "mode_low_rate": "2", "mode_standby": "0",
          "1440P120Hz": "0", "1440P90Hz": "1", "1440P60Hz": "2"
        }, {
          "device": "venus", "name": "Mi 11", "enable": true,
          "mode_high_rate": "0", "mode_low_rate": "2", "mode_standby": "0",
          "1440P120Hz": "0", "1440P90Hz": "1", "1440P60Hz": "2"
        }, {
          "device": "OnePlus9", "enable": true,
          "mode_high_rate": "1", "mode_low_rate": "0"
        }, {
          "device": "zijin", "name": "Mi 12 Lite / Civi 1s", "enable": true,
          "mode_high_rate": "1", "mode_low_rate": "0"
        },
        {
          "device": "RE58B2L1", "name": "Realme GT2(RMX3311/RMX3312)", "enable": true, "mode_high_rate": "10", "mode_low_rate": "1",
          "1080P120Hz": "0", "1080P90Hz": "2", "1080P60Hz": "1"
        },
        { "device": "RMX2202CN", "name": "Realme RMX2202CN",  "enable": true, "mode_high_rate": "1", "mode_low_rate": "0" },
        { "device": "meizu18", "name": "Meizu18th",  "enable": true, "mode_high_rate": "1", "mode_middle_rate": "2", "mode_low_rate": "3" }
      ]
    },
    "cpus_control": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_cpus_control"
    },
    "limiter": {
      "logger": false,
      "active_interval": 20,
      "inactive_interval": 150,
      "params": [
        { "id": "p1:cpu4", "max_freq": 1766400, "min_freq": 1555200, "margin_mhz": 350 },
        { "id": "p1:cpu7", "max_freq": 1785600, "min_freq": 1555200, "margin_mhz": 300 },

        { "id": "p2:cpu4", "max_freq": 2112000, "min_freq": 1881600, "margin_mhz": 550 },
        { "id": "p2:cpu7", "max_freq": 2265600, "min_freq": 1785600, "margin_mhz": 400 },

        { "id": "p3:cpu4", "max_freq": 2342400, "min_freq": 2112000, "margin_mhz": 550 },
        { "id": "p3:cpu7", "max_freq": 2496000, "min_freq": 2265600, "margin_mhz": 450 },

        { "id": "p4:cpu4", "max_freq": 2496000, "min_freq": 2342400, "margin_mhz": 550 },
        { "id": "p4:cpu7", "max_freq": 2841600, "min_freq": 2496000, "margin_mhz": 550 },

        { "id": "inactive:cpu0", "max_freq": 1804800, "min_freq": 800000, "margin_mhz": 240 },
        { "id": "inactive:cpu4", "max_freq": 1670400, "margin_mhz": 200 },
        { "id": "inactive:cpu7", "max_freq": 1785600, "margin_mhz": 200 },

        { "id": "idle:cpu0", "max_freq": 1804800, "min_freq": 800000, "margin_mhz": 200 },
        { "id": "idle:cpu4", "max_freq": 1440000, "margin_mhz": 100 },
        { "id": "idle:cpu7", "max_freq": 1670400, "margin_mhz": 120 }
      ]
    }
  },
  "presets": {
    "reset": [
      ["@msm_reset"],
      ["@governor", "schedutil"]
    ],
    "hispeed_freq": [
      ["/sys/devices/system/cpu/cpufreq/policy0/schedutil/hispeed_freq"],
      ["/sys/devices/system/cpu/cpufreq/policy4/schedutil/hispeed_freq"],
      ["/sys/devices/system/cpu/cpufreq/policy7/schedutil/hispeed_freq"]
    ],
    "hispeed_load": [
      ["/sys/devices/system/cpu/cpufreq/policy0/schedutil/hispeed_load"],
      ["/sys/devices/system/cpu/cpufreq/policy4/schedutil/hispeed_load"],
      ["/sys/devices/system/cpu/cpufreq/policy7/schedutil/hispeed_load"]
    ],
    "sched_config": [
      ["/proc/sys/kernel/sched_downmigrate"],
      ["/proc/sys/kernel/sched_upmigrate"],
      ["/proc/sys/kernel/sched_group_downmigrate"],
      ["/proc/sys/kernel/sched_group_upmigrate"]
    ],
    "sched_boost": [
      ["/proc/sys/kernel/sched_boost_top_app"],
      ["/proc/sys/kernel/sched_boost"],
      ["/dev/stune/top-app/schedtune.prefer_idle"],
      ["/dev/stune/top-app/schedtune.boost"]
    ],
    "min_freq": [ ["$cpu_min_0"], ["$cpu_min_4"], ["$cpu_min_7"] ],
    "max_freq": [ ["$cpu_max_0"], ["$cpu_max_4"], ["$cpu_max_7"] ],
    "cores_force_on": [
      ["$core_ctl_7", "0"], ["$max_cpus_cpu7", "1"], ["$min_cpus_cpu7", "1"]
    ],
    "cores_force_off": [
      ["$core_ctl_7", "0"], ["$min_cpus_cpu7", "0"], ["$max_cpus_cpu7", "0"]
    ],
    "cores_auto_off": [
      ["$core_ctl_7", "1"], ["$min_cpus_cpu7", "0"], ["$max_cpus_cpu7", "1"]
    ],
    "bw_max": [
      ["$llcc_ddr_max", "#12191"], ["$cpu_llcc_max", "#15258"],
      ["$llcc_ddr_min",   "#762"], ["$cpu_llcc_min",    "#2288"]
    ],
    "bw_low": [
      ["$llcc_ddr_max", "#6515"], ["$cpu_llcc_max", "#9155"],
      ["$llcc_ddr_min",  "#762"], ["$cpu_llcc_min", "#2288"]
    ],
    "comm_active": [
      ["$cpu_max_0", "1804800"],
      ["@high_rate", "on"],
      ["@preset", "cores_force_on"],
      ["@sched_limit", "0 0", "0 0", "0 0"],
      ["@cpuset", "1-3", "1-3", "1-6", "0-7"],
      ["@uclamp", "0.00~20.00", "1.00~max", "1.00~max"]
    ],
    "comm_inactive": [
      ["@values", "min_freq", "300000", "710400", "710400"],
      ["@repeat", "sched_config", "85 85", "95 95", "150", "400"],
      ["@sched_limit", "0 0", "0 4000", "0 4000"],
      ["@cpuset", "0-2", "1-3", "1-6", "0-7"],
      ["@uclamp", "0.00~20.00", "0.00~max", "0.00~max"]
    ],
    "comm_gaming": [
      ["@limiters", "NONE"],
      ["@preset", "bw_max"],
      ["@uclamp", "0.00~20.00", "1.00~max", "1.00~max"],
      ["@cpuset", "0-2", "1-3", "0-5", "0-7"]
    ],

    "powersave_active": [
      ["@limiters", "p1:cpu4", "p1:cpu7"],
      ["@preset", "comm_active"],
      ["@values", "min_freq", "1094400", "844800", "806400"],
      ["@repeat", "sched_config", "75 75", "85 85", "150", "400"]
    ],
    "powersave_inactive": [
      ["@limiters", "idle:cpu0", "idle:cpu4"],
      ["@high_rate", "auto"],
      ["@preset", "cores_force_off"],
      ["@preset", "comm_inactive"]
    ],

    "balance_active": [
      ["@limiters", "p2:cpu4", "p2:cpu7"],
      ["@preset", "comm_active"],
      ["@values", "min_freq", "1209600", "960000", "806400"],
      ["@repeat", "sched_config", "75 75", "85 85", "150", "400"]
    ],
    "balance_inactive": [
      ["@limiters", "inactive:cpu0", "inactive:cpu4", "inactive:cpu7"],
      ["@preset", "comm_inactive"],
      ["@preset", "cores_auto_off"],
      ["@high_rate", "auto"]
    ],

    "performance_active": [
      ["@limiters", "p3:cpu4", "p3:cpu7"],
      ["@preset", "comm_active"],
      ["@values", "min_freq", "1401600", "1075200", "806400"],
      ["@repeat", "sched_config", "70 70", "80 85", "150", "400"]
    ],
    "performance_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "comm_inactive"],
      ["@values", "max_freq", "1804800", "1996800", "2150400"],
      ["@high_rate", "auto"]
    ],

    "fast_active": [
      ["@limiters", "p4:cpu4", "p4:cpu7"],
      ["@preset", "comm_active"],
      ["@values", "min_freq", "1497600", "1440000", "1190400"],
      ["@repeat", "sched_config", "65 65", "75 78", "200", "400"]
    ],
    "fast_inactive": [
      ["@limiters", "NONE"],
      ["@preset", "comm_inactive"],
      ["@values", "max_freq", "1804800", "2112000", "2265600"],
      ["@high_rate", "auto"]
    ]
  },
  "schemes": {
    "powersave": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "powersave_active"],
        ["@values", "hispeed_freq", "1209600", "960000", "960000"],
        ["@values", "hispeed_load", "90", "90", "90"],
        ["@values", "sched_boost", "0", "0", "0", "0"],
        ["@gpu_freq", "min", "608000"],
        ["@preset", "bw_low"]
      ],
      "app": [
        ["@gpu_freq", "min", "608000"],
        ["@high_rate", "off", "3000"]
      ],
      "game": [
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "1209600", "1804800"],
        ["@cpu_freq", "cpu4", "710400", "1881600"],
        ["@cpu_freq", "cpu7", "844800", "1900800"],
        ["@preset", "perf_normal"],
        ["@gpu_freq", "min", "550000"],
        ["@sched_limit", "0 0", "0 4000", "0 4000"]
      ]
    },
    "balance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "balance_active"],
        ["@values", "hispeed_freq", "1497600", "1209600", "1190400"],
        ["@values", "hispeed_load", "90", "90", "90"],
        ["@values", "sched_boost", "1", "0", "0", "0"],
        ["@high_rate", "on"],
        ["@preset", "bw_max"]
      ],
      "app": [
        ["@gpu_freq", "min", "540000"]
      ],
      "game": [
        ["$mi_thermal", "10"],
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "1209600", "1804800"],
        ["@cpu_freq", "cpu4", "844800", "2112000"],
        ["@cpu_freq", "cpu7", "844800", "2265600"],
        ["@preset", "perf_normal"],
        ["@gpu_freq", "min", "608000"],
        ["@sched_limit", "0 0", "0 0", "0 0"]
      ]
    },
    "performance": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "performance_active"],
        ["@uclamp", "0.00~max", "0.00~max", "1.00~max"],
        ["@sched_limit", "0 0", "0 0", "0 0"],
        ["@values", "hispeed_freq", "1497600", "1440000", "1420800"],
        ["@values", "hispeed_load", "87", "87", "87"],
        ["@values", "sched_boost", "1", "0", "0", "0"],
        ["@high_rate", "on"],
        ["@preset", "bw_max"]
      ],
      "app": [
        ["@gpu_freq", "min", "608000"]
      ],
      "game": [
        ["$mi_thermal", "10"],
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "1305600", "1804800"],
        ["@cpu_freq", "cpu4", "960000", "2419200"],
        ["@cpu_freq", "cpu7", "844800", "2496000"],
        ["@preset", "perf_normal"],
        ["@gpu_freq", "min", "738000"],
        ["@sched_limit", "4000 0", "4000 0", "4000 0"]
      ]
    },
    "fast": {
      "call": [
        ["@preset", "reset"],
        ["@preset", "fast_active"],
        ["@values", "hispeed_freq", "1497600", "1555200", "1670400"],
        ["@values", "hispeed_load", "85", "85", "85"],
        ["@values", "sched_boost", "1", "0", "1", "10"],
        ["@high_rate", "on"],
        ["@preset", "bw_max"]
      ],
      "app": [
        ["@gpu_freq", "min", "778000"]
      ],
      "game": [
        ["$mi_thermal", "13"],
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "1401600", "1804800"],
        ["@cpu_freq", "cpu4", "1209600", "2419200"],
        ["@cpu_freq", "cpu7", "1075200", "2841600"],
        ["@preset", "perf_active"],
        ["@gpu_freq", "min", "778000"],
        ["@sched_limit", "4000 0", "4000 0", "4000 0"]
      ]
    },
    "pedestal": {
      "call": [
        ["@preset", "reset"],
        ["@uclamp", "0.00~max", "30.00~max", "30.00~max"],
        ["@sched_limit", "0 0", "0 0", "0 0"],
        ["@repeat", "sched_config", "62 40", "70 52", "300", "400"],
        ["@values", "hispeed_freq", "1497600", "1555200", "1670400"],
        ["@values", "hispeed_load", "80", "80", "80"],
        ["@values", "sched_boost", "1", "0", "1", "40"],
        ["@gpu_freq", "443000", "max"],
        ["@high_rate", "on"],
        ["@preset", "bw_max"]
      ],
      "app": [
        ["@gpu_freq", "min", "738000"]
      ],
      "game": [
        ["$mi_thermal", "10"],
        ["@preset", "comm_gaming"],
        ["@cpu_freq", "cpu0", "1708800", "1804800"],
        ["@cpu_freq", "cpu4", "1766400", "2419200"],
        ["@cpu_freq", "cpu7", "1785600", "max"],
        ["@preset", "perf_active"],
        ["@gpu_freq", "min", "778000"],
        ["@sched_limit", "4000 0", "4000 0", "4000 0"]
      ]
    }
  },
  "apps": [
    {
      "friendly": "视频/短视频/滚动优化/社交/",
      "categories": ["Video", "ShortVideo", "ScrollOpt", "Music", "Reader", "IM"],
      "import": "_Apps.json"
    },
    {
      "friendly": "极速响应",
      "categories": ["Launcher"],
      "import": "_Launcher.json"
    },
    {
      "friendly": "扫码",
      "categories": ["Scanner"],
      "import": "_Scanner.json"
    },
    {
      "friendly": "相机",
      "categories": ["Camera"],
      "import": "_Camera.json"
    },
    {
      "friendly": "狗屎/毒瘤",
      "categories": ["MiniProgram"],
      "import": "_MiniProgram.json"
    },
    {
      "friendly": "备份还原",
      "packages": ["com.miui.huanji", "com.miui.backup"],
      "call": [
        ["@uclamp", "0.00~max", "max~max", "max~max"],
        ["@high_rate", "off", "2000"]
      ]
    },
    {
      "friendly": "All",
      "packages": ["*"],
      "import": "_Apps.json"
    }
  ],
  "games": [
    {
      "friendly": "原神",
      "categories": ["GenshinImpact"],
      "import": "_GenshinImpact.json"
    },
    {
      "friendly": "幻塔",
      "packages": [
        "com.dw.h5yvzr.yt",
        "com.pwrd.hotta.laohu",
        "com.hottagames.hotta.bilibili",
        "com.hottagames.hotta.mi"
      ],
      "import": "_Hotta.json"
    },
    {
      "friendly": "机动都市阿尔法",
      "packages": ["com.netease.jddsaef", "com.netease.g93na", "com.netease.g93natw"],
      "cpuset": {
        "heavy_thread": "Thread-",
        "heavy_cores": "7",
        "other": "0-6"
      }
    },
    {
      "friendly": "LOL",
      "packages": ["com.tencent.lolm"],
      "import": "_LOLM.json"
    },
    {
      "friendly": "王者荣耀/LOL",
      "packages": ["com.tencent.tmgp.sgame", "com.garena.game.kgtw"],
      "import": "_SGAME.json"
    },
    {
      "friendly": "和平精英",
      "packages": ["com.tencent.tmgp.pubgmhd",  "com.tencent.ig"],
      "import": "_PUBG.json"
    },
    {
      "friendly": "QQ飞车",
      "packages": ["com.tencent.tmgp.speedmobile"],
      "import": "_SpeedMobile.json"
    },
    {
      "friendly": "APEX",
      "packages": ["com.levelinfinite.apexlegendsmobile", "com.ea.gp.apexlegendsmobilefps", "com.tw.fivexgames.apexlegendsmobile"],
      "import": "_APEX.json"
    },
    {
      "friendly": "All",
      "packages": ["*"]
    }
  ]
}