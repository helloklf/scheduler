        "heavy_thread": "Thread-",
        "heavy_cores": "7",
        "other": "0-6"
      }
    },
    {
      "friendly": "原神",
      "categories": ["GenshinImpact"],
      "import": "_GenshinImpact.json"
    },
    {
      "friendly": "幻塔",
      "packages": [
        "com.dw.h5yvzr.yt",
        "com.pwrd.hotta.laohu",
        "com.hottagames.hotta.bilibili",
        "com.hottagames.hotta.mi"
      ],
      "cpuset": {
        "interval": 2000,
        "comm": {
          "7": ["GameThread"],
          "4-7": ["RenderThread", "RHIThread"]
        },
        "cpuset": "0-6"
      }
    },
    {
      "friendly": "和平精英",
      "packages": ["com.tencent.tmgp.pubgmhd",  "com.tencent.ig"],
      "sensors": [],
      "cpuset": {
        "interval": 1000,
        "heavy_thread": "RenderThread",
        "heavy_cores": "7",
        "comm": {
          "4-6": ["Thread-", "TaskGraphNP"]
        },
        "other": "0-6"
      }
    },
    {
      "friendly": "王者荣耀/LOL",
      "packages": ["com.tencent.tmgp.sgame"],
      "import": "_SGAME.json"
    },
    {
      "friendly": "QQ飞车",
