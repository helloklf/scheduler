          "sensors": [
            {
              "sensor": "mtktscpu",
              "logger": false,
              "disable": false,
              "interval": 200,
              "rules": [
                {
                  "threshold": [-1, 110000],
                  "enter": [
                    ["$max_cpu_freq", "1 1451000"],
                    ["$max_cpu_freq", "2 1482000"]
                  ]
                },
                {
                  "threshold": [110000, 105000],
                  "enter": [
                    ["$max_cpu_freq", "1 1740000"],
                    ["$max_cpu_freq", "2 1998000"]
                  ]
                },
                {
                  "threshold": [105000, 100000],
                  "enter": [
                    ["$max_cpu_freq", "1 1985000"],
                    ["$max_cpu_freq", "2 2284000"]
                  ]
                },
                {
                  "threshold": [100000, 95000],
                  "enter": [
                    ["$max_cpu_freq", "1 1985000"],
                    ["$max_cpu_freq", "2 2463000"]
                  ]
                },
                {
                  "threshold": [85000, -1],
                  "enter": [
                    ["$max_cpu_freq", "1 2354000"],
                    ["$max_cpu_freq", "2 2713000"]
                  ]
                }
              ]
            }
          ],
          "booster": {
            "events": ["touch", "buttons", "qpnp_pon", "synaptics", "uinput-fpc", "gpio-keys"],
            "duration": 30000,
            "enter": [
              ["$deisolation", "4|5|6|7"]
            ],
            "exit": [
              ["$deisolation", "4|5|6|7"]
            ]
          }
        },
        {
          "mode": ["fast", "pedestal"],
          "logger": false,
          "sensors": [
            {
              "sensor": "mtktscpu",
              "logger": false,
              "disable": false,
              "interval": 200,
              "rules": [
                {
                  "threshold": [-1, 113000],
                  "enter": [
                    ["$max_cpu_freq", "1 1451000"],
                    ["$max_cpu_freq", "2 1482000"]
                  ]
                },
                {
                  "threshold": [-1, 105000],
