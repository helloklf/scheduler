{
  "friendly": "王者荣耀/LOL",
  "packages": ["com.tencent.tmgp.sgame", "com.garena.game.kgtw", "com.tencent.lolm"],
  "sensors": [],
  "affinity": {
    "interval": 1000,
    "comm": {
      "80": ["UnityMain"],
      "F0": ["UnityPreload"],
      "70": ["NativeThread", "Thread-","UnityGfx", "Worker Thread", "CoreThread"],
      "7F": ["Audio", "NDK Media", "GVoice", "FMOD mixer", "FMOD stream", "ff_read"]
    },
    "other": "7F"
  },
  "call": [
    ["@thermal_disguise", "true"],
    ["$mi_thermal", "13"],
    ["@high_rate", "on"],
    ["@uclamp", "0.00~1.00", "0.00~max", "2.00~max"]
  ],
  "modes": [{
    "mode": ["powersave"],
    "call": [
      ["@classical", "cpu0", "70", "60", "6", "1"],
      ["@classical", "cpu4", "75", "63", "2", "2"],
      ["@classical", "cpu7", "73", "62", "2", "2"],
      ["$core_ctl_4", "0"], ["$core_ctl_7", "0"],
      ["@gpu_freq", "min", "315000"],
      ["@sched_limit", "0 0", "0 2000", "0 0"]
    ],
    "affinity": {
      "interval": 1000,
      "comm": {
        "80": ["UnityMain"],
        "F0": ["UnityPreload"],
        "70": ["NativeThread", "Thread-", "UnityGfx"],
        "7F": ["Audio", "NDK Media", "GVoice", "FMOD mixer", "FMOD stream", "ff_read", "Worker Thread", "CoreThread"]
      },
      "other": "7F"
    },
    "sensors": [{
      "sensor": "fps",
      "interval": 5000,
      "props": ["$cpu_min_0", "$cpu_min_4", "$cpu_min_7",  "$cpu_max_0", "$cpu_max_4", "$cpu_max_7"],
