{
  "friendly": "视频应用",
  "packages": [
    "tv.danmaku.bili",
    "com.tencent.qqlive",
    "com.ss.android.article.video",
    "com.qiyi.video",
    "com.qiyi.video.lite",
    "com.qiyi.video.sdkplayer",
    "com.dianshijia.newlive",
    "com.duowan.kiwi",
    "air.tv.douyu.android",
    "com.douyu.rush",
    "com.youku.phone",
    "org.telegram.messenger",
    "com.hunantv.imgo.activity"
  ],
  "modes": [{
    "mode": ["powersave"],
    "call": [
      ["@classical", "cpu0", "70", "60", "3", "1"],
      ["@classical", "cpu4", "85", "68", "3", "1"],
      ["@classical", "cpu7", "80", "70", "3", "1"]
    ],
    "booster": {
      "events": ["touch", "buttons", "qpnp_pon", "synaptics", "uinput-fpc", "gpio-keys"],
      "duration": 2000,
      "idle_delay": 3000,
      "enter": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["$cpuset_top", "0-7"],
        ["$cpuset_fg", "0-7"],
        ["$cpuset_bg", "0-3"],
        ["@uclamp", "0.00~max", "1.00~max", "1.00~max"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["@uclamp", "0.00~0.00", "0.00~1.00", "0.00~5.00"],
        ["$cpu_max_0", "1708800"],
        ["$cpu_max_4", "1555200"],
        ["$cpu_max_7", "1555200"],
        ["$stune_top_boost", "0"],
        ["$stune_top_perf", "0"],
        ["$cpuset_top", "0-6"],
        ["$cpuset_fg", "0-6"],
        ["$cpuset_bg", "0"],
        ["@high_rate", "off"]
      ]
    }
  }, {
    "mode": ["balance"],
    "booster": {
      "events": ["touch", "buttons", "qpnp_pon", "synaptics", "uinput-fpc", "gpio-keys"],
      "duration": 2000,
      "idle_delay": 4000,
      "enter": [
        ["$cpuset_top", "0-7"],
        ["$cpuset_fg", "0-7"],
        ["@uclamp", "0.00~max", "1.00~max", "1.00~max"]
      ],
      "exit": [
        ["@uclamp", "0.00~0.00", "0.00~1.00", "0.00~10.00"],
        ["$stune_top_boost", "0"],
        ["$stune_top_perf", "0"],
        ["$cpuset_top", "0-6"],
        ["$cpuset_fg", "0-6"]
      ]
    }
  }, {
    "mode": ["performance", "fast"],
    "booster": {
      "events": ["touch", "buttons", "qpnp_pon", "synaptics", "uinput-fpc", "gpio-keys"],
      "duration": 2000,
      "enter": [
        ["$cpuset_top", "0-7"],
        ["$stune_top_boost", "10"],
        ["$stune_top_perf", "1"],
        ["@uclamp", "0.00~max", "1.00~max", "1.00~max"]
      ],
      "exit": [
        ["@uclamp", "0.00~20.00", "0.00~max", "0.00~max"]
      ]
    }
  }]
}