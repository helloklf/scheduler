{
  "friendly": "小说阅读",
  "packages": [
    "io.legado.app.release",
    "com.duokan.reader",
    "com.ruguoapp.jike",
    "com.ruanmei.ithome"
  ],
  "stop_on": ["leave"],
  "sensors": [],
  "modes": [{
    "mode": ["powersave", "balance"],
    "affinity": {
      "comm": {
        "F0": ["RenderThread", "hwuiTask", "GLThread"]
      },
      "other": "7f"
    },
    "booster": {
      "events": ["touch", "buttons", "qpnp_pon", "synaptics", "uinput-fpc", "gpio-keys"],
      "duration": 1500,
      "idle_delay": 1500,
      "enter": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["$cpuset_top", "0-7"],
        ["@uclamp", "0.00~max", "1.00~max", "1.00~max"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["$cpuset_top", "0-6"],
        ["$cpuset_fg", "0-6"],
        ["$cpu_max_0", "1708800"],
        ["$cpu_max_4", "1555200"],
        ["$cpu_max_7", "1555200"],
        ["@uclamp", "0.00~1.00", "0.00~1.00", "0.00~10.00"],
        ["@high_rate", "off"]
      ]
    }
  },{
    "mode": ["performance", "fast"],
    "affinity": {
      "comm": {
        "F0": ["RenderThread", "hwuiTask", "GLThread"]
      },
      "other": "7f"
    },
    "booster": {
      "events": ["touch", "buttons", "qpnp_pon", "synaptics", "uinput-fpc", "gpio-keys"],
      "duration": 1500,
      "idle_delay": 1500,
      "enter": [
        ["$cpuset_top", "0-7"],
        ["@uclamp", "0.00~max", "1.00~max", "1.00~max"],
        ["@high_rate", "on"]
      ],
      "exit": [
        ["@uclamp", "0.00~20.00", "0.00~max", "0.00~max"],
        ["$cpuset_top", "0-6"],
        ["$cpuset_fg", "0-6"],
        ["@high_rate", "off"]
      ]
    }
  }]
}