{
  "platform": "lahaina",
  "platform_name": "888/888+",
  "framework": 130,
  "alias": {
    "gpu_max_khz": "/sys/class/kgsl/kgsl-3d0/devfreq/max_freq",
    "mi_thermal": "/sys/class/thermal/thermal_message/sconfig"
  },
  "schemes": {
    "powersave": {
      "call": [
        ["$core_ctl_4", "1"],
        ["$core_ctl_7", "1"],
        ["@thermal_disguise", "false"],
        ["@gpu_freq", "min", "491MHz"],
        ["@high_rate", "off", "3000"]
      ]
    },
    "balance": {
      "call": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["@thermal_disguise", "false"],
        ["@gpu_freq", "min", "540000"],
        ["@high_rate", "on"]
      ]
    },
    "performance": {
      "call": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["@thermal_disguise", "true"],
        ["@gpu_freq", "min", "608000000"],
        ["@high_rate", "on"]
      ]
    },
    "fast": {
      "call": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["@thermal_disguise", "true"],
        ["@gpu_freq", "min", "738000"],
        ["@high_rate", "on"]
      ]
    },
    "pedestal": {
      "call": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["@thermal_disguise", "true"],
        ["@gpu_freq", "491000", "max"],
        ["$mi_thermal", "10"],
        ["@high_rate", "on"]
      ]
    }
  },
  "features": {
    "high_rate": {
      "enable": true,
      "enable_control": "/data/local/tmp/scene_refresh_rate",
      "device_policy": [{
        "device": "star", "name": "Mi 11 Pro/Ultra", "enable": true,
        "mode_high_rate": "0", "mode_low_rate": "2", "mode_standby": "0",
        "1440P120Hz": "0", "1440P90Hz": "1", "1440P60Hz": "2"
      }, {
        "device": "mars", "name": "Mi 11 Pro/Ultra", "enable": true,
        "mode_high_rate": "0", "mode_low_rate": "2", "mode_standby": "0",
        "1440P120Hz": "0", "1440P90Hz": "1", "1440P60Hz": "2"
      }, {
        "device": "venus", "name": "Mi 11", "enable": true,
        "mode_high_rate": "0", "mode_low_rate": "2", "mode_standby": "0",
        "1440P120Hz": "0", "1440P90Hz": "1", "1440P60Hz": "2"
      }, {
        "device": "odin", "name": "Mix 4", "enable": false,
        "mode_high_rate": "1", "mode_low_rate": "0",
        "1080P120Hz": "1", "1080P90Hz": "2", "1080P60Hz": "0"
      }, {
        "device": "OnePlus9", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0"
      }, {
        "device": "zijin", "name": "Mi 12 Lite / Civi 1s", "enable": true,
        "mode_high_rate": "1", "mode_low_rate": "0"
      }]
    }
  },
  "apps": [
