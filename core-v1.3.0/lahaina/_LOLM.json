{
  "friendly": "王者荣耀/LOL",
  "packages": ["com.tencent.tmgp.sgame", "com.garena.game.kgtw", "com.tencent.lolm"],
  "sensors": [],
  "affinity": {
    "interval": 1000,
    "comm": {
      "80": ["UnityMain"],
      "F0": ["UnityPreload"],
      "70": ["NativeThread", "Thread-", "UnityGfx"],
      "7F": ["Audio", "NDK Media", "GVoice", "FMOD mixer", "FMOD stream", "ff_read", "Worker Thread", "CoreThread"]
    },
    "other": "7F"
  },
  "call": [
    ["@thermal_disguise", "true"],
    ["$mi_thermal", "13"],
    ["@high_rate", "on"],
    ["@uclamp", "0.00~1.00", "0.00~max", "1.00~max"]
  ],
  "modes": [{
    "mode": ["powersave"],
    "call": [
      ["@classical", "cpu0", "70", "60", "6", "1"],
      ["@classical", "cpu4", "83", "72", "2", "1"],
      ["@classical", "cpu7", "83", "72", "2", "1"],
      ["$core_ctl_4", "0"], ["$core_ctl_7", "0"],
      ["@gpu_freq", "min", "315000"],
      ["@sched_limit", "1000 0", "0 2000", "0 0"]
    ],
    "sensors": [{
      "sensor": "fps",
      "interval": 5000,
      "props": ["$cpu_min_0", "$cpu_min_4", "$cpu_min_7",  "$cpu_max_0", "$cpu_max_4", "$cpu_max_7"],
      "rules": [{
        "threshold": [-1, 100],
        "values": ["1094400", "1075200", "960000",    "1612800", "2112000", "2150400"]
      }, {
        "threshold": [99, 75],
        "values": ["1094400", "960000", "844800",    "1612800", "1996800", "1900800"]
      }, {
        "threshold": [70, -1],
        "values": ["902400", "844800", "844800",    "1612800", "1881600", "1900800"]
      }]
    }]
  },{
