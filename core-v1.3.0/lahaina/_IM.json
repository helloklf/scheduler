{
  "friendly": "社交APP",
  "packages": [
    "com.tencent.mobileqq",
    "com.tencent.mm",
    "com.tencent.tim",
    "org.telegram.messenger"
  ],
  "stop_on": [],
  "sensors": [],
  "modes": [{
    "mode": ["powersave"],
    "call": [
      ["@classical", "cpu0", "70", "60", "3", "1"],
      ["@classical", "cpu4", "85", "68", "3", "1"],
      ["@classical", "cpu7", "80", "70", "3", "1"]
    ],
    "booster": {
      "events": ["touch", "buttons", "qpnp_pon", "synaptics", "uinput-fpc", "gpio-keys"],
      "duration": 2000,
      "idle_delay": 3000,
      "enter": [
        ["$core_ctl_4", "0"],
        ["$core_ctl_7", "0"],
        ["$cpuset_top", "0-7"],
        ["$cpuset_fg", "0-7"],
        ["@cpufreq", "cpu0", "960000", "1708800"],
        ["@cpufreq", "cpu4", "844800", "1555200"],
        ["@cpufreq", "cpu7", "844800", "1708800"],
        ["@uclamp", "0.00~max", "0.10~max", "0.25~max"],
        ["@high_rate", "on"]
      ],
      "batter": 4,
      "infinite": [
        ["@uclamp", "0.00~max", "1.00~max", "1.00~max"],
        ["$stune_top_perf", "1"]
      ],
      "exit": [
        ["$core_ctl_4", "1"],
        ["$core_ctl_7", "1"],
        ["$stune_top_boost", "0"],
        ["$stune_top_perf", "0"],
        ["$cpuset_top", "0-6"],
        ["$cpuset_fg", "0-6"],
